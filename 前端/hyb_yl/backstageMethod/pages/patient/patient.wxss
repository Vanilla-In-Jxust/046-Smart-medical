.W_seek_box {
    width: 100%;
    height: 118rpx;
    background: #fff;
}

.W_seek_input {
    width: 550rpx;
    height: 80rpx;
    background: #f5f5f7;
    border-radius: 10rpx;
    float: left;
    margin-left: 30rpx;
    margin-top: 20rpx;
    font-size: 30rpx;
    position: relative;
}

.W_seek_input1 {
    width: 440rpx;
    height: 80rpx;
    background: #f5f5f7;
    border-radius: 10rpx;
    float: left;
    margin-left: 80rpx;
    font-size: 32rpx;
}

.W_seek_input_img {
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    top: 20rpx;
    left: 30rpx;
}

.seet {
    width: 140rpx;
    height: 80rpx;
    background: linear-gradient(to right,#69f,#3b4ac9);
    float: left;
    font-size: 32rpx;
    line-height: 80rpx;
    text-align: center;
    color: #fff;
    margin-left: 1rpx;
    margin-top: 20rpx;
    border-radius: 8rpx;
}

.W_li {
    width: 100%;
    height: 160rpx;
    background: #fff;
    float: left;
    border-bottom: 1rpx solid #ccc;
}

.W_li_img_box {
    width: 140rpx;
    height: 160rpx;
    float: left;
}

.W_li_img {
    width: 100rpx;
    height: 100rpx;
    display: block;
    margin: 30rpx auto;
    border-radius: 100rpx;
    background: #999;
}

.W_li_text {
    width: 400rpx;
    height: 160rpx;
    float: left;
}

.W_li_text_name {
    width: 100%;
    height: 50rpx;
    margin-top: 60rpx;
}

.W_li_text_namea {
    float: left;
    color: #333;
    line-height: 40rpx;
    font-size: 30rpx;
    max-width: 230rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.W_li_text_namea_img {
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin-left: 20rpx;
}

.W_li_text_nameDate {
    color: #999;
    font-size: 30rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin-left: 20rpx;
}

.W_li_textA {
    font-size: 30rpx;
    color: #999;
    width: 100%;
    margin-top: 20rpx;
}

.remarkName {
    font-size: 24rpx;
    color: #999;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.movement {
    height: 160rpx;
    line-height: 160rpx;
    font-size: 30rpx;
    float: right;
}

.movement_a {
    width: 60rpx;
    height: 60rpx;
    float: right;
    margin-top: 50rpx;
    margin-right: 30rpx;
}

.distance {
    display: flex;
    align-items: center;
    padding-right: 20rpx;
    box-sizing: border-box;
}

.distance image {
    width: 30rpx;
    height: 30rpx;
}

.distance text {
    font-size: 24rpx;
    padding-left: 10rpx;
    box-sizing: border-box;
}