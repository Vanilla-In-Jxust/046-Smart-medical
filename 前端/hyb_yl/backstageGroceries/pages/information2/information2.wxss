}

.said_image {
    height: 100rpx;
    width: 100rpx;
    border-radius: 50%;
}

.said_content {
    flex: 1;
    margin-left: 30rpx;
}

.content_title {
    display: flex;
    justify-content: space-between;
}

.content_content {
    margin-top: 20rpx;
}

.content_img_list {
    margin-top: 15rpx;
}

.content_img {
    margin-top: 15rpx;
    margin-right: 7rpx;
    height: 180rpx;
    width: 180rpx;
}

.content_said {
    margin-top: 30rpx;
    width: 560rpx;
    height: 104rpx;
    background-color: #f0f0f0;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 20rpx 0 15rpx;
    box-sizing: border-box;
}

.content_said_img_view {
    position: relative;
}

.content_said_img {
    height: 72rpx;
    width: 96rpx;
}

.content_said_img_view_img {
    width: 50rpx;
    height: 50rpx;
}

.content_said_content {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    flex: 1;
    margin-left: 20rpx;
}

.content_title_title {
    display: flex;
    align-items: flex-end;
}

.wzname {
    width: 150rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    float: left;
}

.wzdright {
    font-size: 28rpx;
    padding: 10rpx;
    height: 100%;
}

.sound {
    display: flex;
    align-items: center;
    padding-top: 10rpx;
}

.sound_view {
    width: 230rpx;
    height: 90rpx;
    background-color: #f0f0f0;
    border-radius: 10rpx;
    position: relative;
    display: flex;
    align-items: center;
}

.sound_view_left {
    position: absolute;
    left: -10rpx;
    top: 25rpx;
    height: 40rpx;
    width: 40rpx;
    transform: rotate(45deg);
    background-color: #f0f0f0;
    border-radius: 4rpx;
}

.sound_view_view {
    display: flex;
    align-items: center;
    flex: 1;
    justify-content: flex-end;
}

.sound_img {
    width: 50rpx;
    height: 50rpx;
    position: relative;
    margin-left: 20rpx;
}

.time_view {
    margin-left: 20rpx;
}

.sound_animation {
    animation: sound 0.5s ease-in both alternate infinite;
}

.sound_img_view {
    overflow: hidden;
    background-color: #f0f0f0;
}

@-webkit-keyframes sound {
    from {
        transform: scale(0.5);
    }

    50% {
        transform: scale(1);
    }

    100% {
        transform: scale(1.1);
    }
}

@keyframes sound {
    from {
        transform: scale(0.5);
    }

    50% {
        transform: scale(1);
    }

    100% {
        transform: scale(1.1);
    }
}

