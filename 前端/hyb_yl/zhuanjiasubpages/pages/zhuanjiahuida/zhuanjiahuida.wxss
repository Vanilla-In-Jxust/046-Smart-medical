page {
    background: #FAFAFA;
    box-sizing: border-box;
    padding: 25rpx;
    padding-bottom: 300rpx!important;
}

view {
    box-sizing: border-box;
}

.ordertime {
    text-align: center;
    font-size: 22rpx;
    color: #A9A9A9;
}

.orderbox {
    width: 100%;
    background: #EBEBEB;
    border-radius: 10rpx;
    padding: 25rpx;
    margin-top: 25rpx;
    font-size: 28rpx;
    color: #9F9F9F;
    line-height: 50rpx;
}

.orderbox text {
    color: #0087E0;
}

.doctorbox {
    background: #fff;
}

.doctor {
    display: flex;
    align-items: center;
    font-size: 32rpx;
    color: #2E2E2E;
}

.doctor image {
    width: 65rpx;
    height: 65rpx;
    margin-right: 20rpx;
    border-radius: 50%;
}

.symptom {
    font-size: 32rpx;
    color: #666666;
    margin-top: 25rpx;
}

.symptom image {
    width: 120rpx;
    height: 120rpx;
    border-radius: 5rpx;
    object-fit: cover;
    margin-right: 20rpx;
}

.buttons {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    margin-top: 25rpx;
}

.buttons view {
    width: 200rpx;
    text-align: center;
    line-height: 60rpx;
    font-size: 26rpx;
    margin-left: 20rpx;
    border-radius: 10rpx;
}

.buttons view:first-child {
    border: 1rpx solid #AFDAF5;
    color: #0087E0;
}

.buttons view:last-child {
    background: #0087E0;
    color: #fff;
}

.yaoming {
    font-size: 28rpx;
    color: #9A9A9A;
    margin-top: 5rpx;
}

.symptom text {
    color: #0087E0;
}

.evaluate {
    width: 90%;
    background: #fff;
    border-radius: 50rpx;
    box-shadow: 0 0 5rpx 5rpx #EFEFEF;
    padding: 25rpx;
    display: flex;
    align-items: center;
    position: fixed;
    bottom: 150rpx;
    left: 5%;
    z-index: 2000;
}

.evaluateimg {
    display: inline-flex;
    align-items: center;
}

.evaluateimg image {
    width: 35rpx;
    height: 35rpx;
    margin-left: 10rpx;
}

.evaluateimg image:first-child {
    margin-left: 0;
}

.evaluatehint {
    font-size: 30rpx;
    color: #D7655B;
    padding: 3rpx 15rpx;
    position: relative;
    border: 1rpx solid #D7655B;
    margin-left: 30rpx;
    border-radius: 8rpx;
}

.evaluatehint::after {
    content: "";
    width: 12rpx;
    height: 12rpx;
    border-top: 1rpx solid #D7655B;
    border-left: 1rpx solid #D7655B;
    position: absolute;
    top: 48%;
    left: -8rpx;
    transform: translateY(-50%) rotate(-45deg);
}

.closeevaluate {
    font-size: 60rpx;
    color: #5D5D5D;
    position: absolute;
    top: 48%;
    right: 25rpx;
    transform: translateY(-50%);
}

.forms {
    width: 100%;
    padding: 20rpx 25rpx;
    background: #F7F7F7;
    border-top: 1rpx solid #EFEFEF;
    display: flex;
    align-items: center;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 200;
}

.formsimgs {
    width: 220rpx;
    text-align: center;
    font-size: 24rpx;
    color: #818181;
}

.formsimgs image {
    width: 55rpx;
    height: 55rpx;
    display: block;
    margin: 0 auto;
}

.forms input {
    border: 1rpx solid #ECECEC;
    border-radius: 10rpx;
    background: #FFFFFF;
    padding: 20rpx;
    font-size: 30rpx;
    color: #666666;
    margin-left: 255rpx;
}

.formsimgs:first-child {
    width: 80rpx;
}

.forms button {
    width: 80rpx;
    font-size: 32rpx;
    color: #5D5D5D;
    padding: 0;
    margin: 0;
    text-align: right;
}

.forms button::after {
    border: none;
}

.zhuiwen {
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 200;
    background: #fff;
}

.zhuiwen-tit {
    line-height: 90rpx;
    color: #B1B1B1;
    font-size: 30rpx;
    position: relative;
    text-align: center;
}

.zhuiwen-tit text {
    font-size: 50rpx;
    color: #5D5D5D;
    position: absolute;
    top: 48%;
    right: 25rpx;
    transform: translateY(-50%);
}

.zhuiwen-textarea {
    width: 100%;
    padding: 25rpx;
    height: 200rpx;
    border-top: 1rpx solid #F7F7F7;
    border-bottom: 1rpx solid #FAFAFA;
}

.zhuiwen-textarea textarea {
    width: 100%;
    height: 100%;
    font-size: 32rpx;
    color: #666666;
}

.sendbox {
    width: 100%;
    padding: 25rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.sendbox .add {
    width: 90rpx;
}

.sendbox button {
    width: 130rpx;
    font-size: 28rpx;
    color: #fff;
    padding: 0;
    margin: 0;
    text-align: center;
    background: #0087E0;
    border-radius: 50rpx;
}

.sendbox button::after {
    border: none;
}

.orderboxss {
    width: 100%;
    background: #E7EBF4;
    border-radius: 10rpx;
    padding: 25rpx;
    margin-top: 25rpx;
}

.titles {
    font-size: 28rpx;
    color: #060606;
}

.mess {
    font-size: 30rpx;
    color: #4B4E51;
    line-height: 60rpx;
}

.imgbox {
    width: 100%;
    margin-top: 25rpx;
}

.imgbox image {
    width: 110rpx;
    height: 110rpx;
    margin-right: 10rpx;
    border-radius: 5rpx;
}

.looks {
    font-size: 24rpx;
    color: #BABBBE;
    margin-top: 25rpx;
}

.ques {
    width: 420rpx;
    height: 80rpx;
    margin-left: 40rpx;
    text-align: center;
    line-height: 80rpx;
    background-color: #0087E0;
    border-radius: 80rpx;
    font-size: 30rpx;
    color: #fff;
}

.lists-item {
    display: flex;
    justify-content: flex-start;
}

.img {
    position: relative;
    margin-right: 30rpx;
}

.close {
    position: absolute;
    right: -15rpx;
    top: -15rpx;
    width: 40rpx;
    height: 40rpx;
}

.icons {
    width: 90rpx;
    height: 90rpx;
}

.sickitem {
    background: #fff;
    border-radius: 10rpx;
    margin-bottom: 26rpx;
    width: 100%;
    display: flex;
    align-items: stretch;
    justify-content: space-between;
    overflow: hidden;
    box-shadow: 0 0 5rpx 5rpx #FAFAFA;
}

.sickitem-left {
    width: 32%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 20rpx;
}

.sickitem-left image {
    width: 70rpx;
    height: 70rpx;
}

.sickiteminfo view:first-child {
    font-size: 30rpx;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.sickiteminfo view:last-child {
    font-size: 26rpx;
    color: #A7A7A7;
    margin-top: 8rpx;
}

.sickitem-center {
    width: 65%;
    padding: 0 20rpx;
    border-left: 1rpx solid #F8F8F8;
    margin: 20rpx 0;
}

.sickitem-center view {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.sickitem-center view:first-child {
    font-size: 30rpx;
    color: #333;
}

.sickitem-center view:nth-child(2) {
    font-size: 24rpx;
    color: #8C8C8C;
    margin: 8rpx 0;
}

.sickitem-center view:last-child {
    font-size: 22rpx;
    color: #8C8C8C;
}

.sickbtn {
    width: 66rpx;
    font-size: 26rpx;
    color: #fff;
    text-align: center;
    padding: 10rpx;
}

view.btn1 {
    background: #2876E6;
}

view.btn2 {
    background: #E67428;
}

view.btn3 {
    background: #8E6EF9;
}

.sickiteminfo {
}

.sickiteminfo view:first-child {
    font-size: 30rpx;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.sickiteminfo view:last-child {
    font-size: 26rpx;
    color: #A7A7A7;
    margin-top: 8rpx;
}