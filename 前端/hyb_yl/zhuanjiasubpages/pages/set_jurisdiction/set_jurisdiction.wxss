page {
    background: #fff;
}

.box {
    box-sizing: border-box;
    padding: 0 24rpx;
}

.cont {
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #fff;
    border-radius: 5rpx;
    padding: 24rpx;
    margin-top: 30rpx;
    font-size: 26rpx;
    color: #333;
}

.c9 {
    color: #999;
}

.rule {
    color: #999;
    font-size: 30rpx;
    margin-top: 30rpx;
}

.switch {
    zoom: 0.8;
}

.btn {
    background: rgb(34,199,131);
    color: #fff;
    white-space: nowrap;
    border-radius: 10rpx;
    padding: 10rpx 20rpx;
    font-size: 26rpx;
}

.c9 {
    color: #999;
}

.fs24 {
    font-size: 24rpx;
}

.c3 {
    color: #333;
}

.fs28 {
    font-size: 28rpx;
}

button::after {
    width: 100%;
    border: 0;
}

button {
    text-align: left;
}