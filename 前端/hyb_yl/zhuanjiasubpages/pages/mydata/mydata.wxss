.mydata-box {
    padding: 24rpx;
}

.mydata-list {
    width: 654rpx;
    padding: 0 24rpx;
    background: #fff;
    border-radius: 30rpx;
    margin-bottom: 24rpx;
}

.mydata-item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 100%;
    height: 88rpx;
    border-bottom: 1rpx solid #EAEAEA;
}

.mydata-item:last-child {
    border-bottom: 0;
}

.mydata-tit {
    font-size: 28rpx;
    color: #333;
}

.mydata-ri {
    display: flex;
    align-items: center;
}

.pho {
    width: 65rpx;
    height: 65rpx;
    border-radius: 50%;
    border: 1rpx solid #0088DE;
}

.next {
    width: 42rpx;
    height: 42rpx;
    margin-left: 20rpx;
}

.mydata-ri>view {
    width: 430rpx;
    text-align: right;
    font-size: 24rpx;
    color: #999;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.mydata-rz {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    text-align: right;
}

.mydata-rz image {
    width: 27rpx;
    height: 27rpx;
    margin-right: 15rpx;
}