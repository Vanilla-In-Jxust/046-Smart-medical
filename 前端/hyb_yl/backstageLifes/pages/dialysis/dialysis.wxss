page {
    background: #f5f5f7;
}

.wrap {
    width: 650rpx;
    margin: 80rpx auto;
    border-radius: 8rpx;
    overflow: hidden;
    background: #fafafa;
    box-shadow: 0px 8px 8px 0px #e0e0e0;
}

.wrap_top {
    width: 100%;
    background-image: linear-gradient(0deg,#7587e6 0%,#3b4ca9 100%);
    box-shadow: inset 0px -1px 0px 0px #e0e0e0;
    border-radius: 8px 8px 0px 0;
    display: flex;
}

.wrap_text {
    width: 480rpx;
    padding-bottom: 30rpx;
}

.wrap_name {
    width: 100%;
    color: #fafafa;
    padding-left: 50rpx;
    box-sizing: border-box;
    font-size: 44rpx;
    margin-top: 48rpx;
}

.wrap_name1 {
    width: 100%;
    color: #fafafa;
    text-indent: 50rpx;
    font-size: 28rpx;
    margin-top: 16rpx;
}

.wrap_name2 {
    width: 100%;
    color: #fafafa;
    padding-left: 50rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    margin-top: 30rpx;
}

.png {
    width: 120rpx;
    height: 120rpx;
    border-radius: 120rpx;
    background: #fff;
    margin-top: 40rpx;
}

.box {
    width: 100%;
    height: 500rpx;
    background: #fff;
    padding-top: 50rpx;
}

canvas {
    width: 360rpx;
    height: 360rpx;
}

.canvas {
    margin: auto;
}

.box_text {
    text-align: center;
    margin-top: 30rpx;
    font-size: 28rpx;
    color: #666;
}

.foot {
    width: 650rpx;
    height: 80rpx;
    background-image: linear-gradient(0deg,#7587e6 0%,#3b4ca9 100%);
    border-radius: 8rpx;
    text-align: center;
    color: #fff;
    font-size: 32rpx;
    line-height: 80rpx;
    margin: auto;
}

.can_img {
    margin: 0 auto;
    height: 360rpx;
    width: 360rpx;
    display: block;
}