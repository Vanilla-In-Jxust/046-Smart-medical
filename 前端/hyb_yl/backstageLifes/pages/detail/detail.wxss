page {
    background: #f5f5f7;
}

.wraptab {
    width: 100%;
    height: 120rpx;
    background: #fff;
    margin-bottom: 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-around;
}

.wzlist {
    line-height: 120rpx;
    color: #333;
    font-size: 32rpx;
    text-indent: 30rpx;
}

.list {
    font-size: 28rpx;
    padding: 30rpx;
    color: #333;
    border-radius: 56rpx;
}

.active {
    color: #3b4ca9;
}

.tablist {
    width: 100%;
    height: 160rpx;
    float: left;
    background: #fff;
    border-bottom: 1rpx #f0f0f0 solid;
    position: relative;
}

.relative {
    position: absolute;
    height: 60rpx;
    width: 100rpx;
    font-size: 24rpx;
    color: #3b4ca9;
    text-align: center;
    line-height: 60rpx;
    top: 100rpx;
    right: 30rpx;
}

.tablist_text {
    width: 100%;
    font-size: 28rpx;
    color: #333;
    text-indent: 30rpx;
    margin-top: 40rpx;
    float: left;
}

.tablist_text1 {
    width: 100%;
    font-size: 24rpx;
    color: #ccc;
    text-indent: 30rpx;
    float: left;
}

.tablist_text2 {
    font-size: 48rpx;
    text-align: right;
    float: right;
    margin-right: 30rpx;
    color: #114fa6;
}

.tablist_text2a {
    font-size: 48rpx;
    text-align: right;
    float: right;
    margin-right: 30rpx;
    color: #4ca724;
}

.tablist_text3 {
    font-size: 24rpx;
    text-align: right;
    float: right;
    margin-right: 30rpx;
}