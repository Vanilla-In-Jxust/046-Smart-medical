page {
    background: #fff;
    padding-top: 60rpx;
}

.featurebox {
    padding-top: 25rpx;
}

view {
    box-sizing: border-box;
}

.name_box {
    display: flex;
    align-items: center;
    margin-bottom: 10rpx;
}

.userhead {
    width: 34rpx;
    height: 34rpx;
    border-radius: 50%;
}

.name_box .name {
    font-size: 32rpx;
    color: #333;
    font-weight: 500;
    margin-left: 12rpx;
}

.cont_item {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 40rpx 0;
    border-bottom: 2rpx solid #eaeaea;
}

.yiyuanBox {
    width: 100%;
    padding: 0 24rpx;
}

.item_img {
    width: 200rpx;
    height: 150rpx;
    position: relative;
    border-radius: 10rpx;
    margin-right: 20rpx;
}

.img_label {
    position: absolute;
    top: 0;
    left: 0;
    background: #0088de;
    color: #fff;
    font-size: 24rpx;
    padding: 5rpx 20rpx;
    border-radius: 10rpx;
}

.df1 {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 26rpx;
    color: #333;
    width: 100%;
}

.item_name {
    font-size: 30rpx;
    font-weight: bold;
    max-width: 75%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    display: flex;
    align-items: center;
}

.city {
    max-width: 70%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.rule {
    color: #999;
    font-size: 24rpx;
}

.item_right {
    width: 65%;
    height: 150rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.c9 {
    color: #999;
}

.num {
    color: rgb(255,159,0);
}

.name_label {
    border: 2rpx solid #0088de;
    color: #0088de;
    font-size: 18rpx;
    border-radius: 5rpx;
    padding: 3rpx 10rpx;
    margin-right: 10rpx;
    font-weight: 400;
}

.tab_box {
    display: flex;
    align-items: center;
    justify-content: space-between;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    padding: 0 20rpx;
    font-size: 28rpx;
    color: #999;
    background: #fff;
    border-bottom: 2rpx solid #eaeaea;
    z-index: 99;
}

.tab {
    padding: 20rpx 0;
    display: flex;
    align-items: center;
}

.heng {
    width: 50rpx;
    height: 6rpx;
    background: transparent;
    border-radius: 5rpx;
    margin: 0 auto;
}

.active {
    font-weight: bold;
    color: #333;
    font-size: 30rpx;
}

.active .heng {
    background: #0088de;
}

.tab_img {
    width: 30rpx;
}

.zhe {
    position: fixed;
    left: 0;
    background: rgba(0,0,0,0.5);
    width: 100%;
    height: 100%;
    top: 0;
}

.fenlei {
    position: absolute;
    top: 80rpx;
    left: 0;
    width: 100%;
    background: #fff;
    padding: 24rpx;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    max-height: 600rpx;
    overflow: hidden;
    overflow-y: auto;
}

.fenlei_item {
    padding: 10rpx 20rpx;
    border: 2rpx solid #eaeaea;
    font-size: 28rpx;
    color: #999;
    background: #f1f1f1;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
    border-radius: 10rpx;
}

.img_active {
    transform: rotate(180deg);
}

.fenlei_active {
    background: #0088de;
    color: #ffff;
}

.tab_text {
    width: 120rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    text-align: center;
}