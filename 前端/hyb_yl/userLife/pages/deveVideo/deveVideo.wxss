page {
    width: 100%;
    height: 100%;
    background: white;
}

.oval {
    padding: 0rpx 20rpx;
}

.videobox {
    width: 100%;
    height: 400rpx;
    background: #666;
}

.videobox video {
    width: 100%;
    height: 100%;
}

.hjTitle {
    padding: 30rpx 0rpx 40rpx;
    font-weight: bold;
}

.doctorArr {
    display: flex;
}

.doctorImg {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    background: red;
    margin-right: 20rpx;
}

.doctorRight {
    flex: 1;
}

.doctorIntr {
    color: #666;
    font-size: 24rpx;
    margin-bottom: 10rpx;
}

.doctorIntr view {
    margin-left: 10rpx;
}

.time {
    font-size: 24rpx;
    color: #999;
}

.attention {
    padding: 4rpx 10rpx;
    border: 1rpx solid #ccc;
    font-size: 26rpx;
    color: #999;
    font-weight: bold;
    border-radius: 10rpx;
}