.banner {
    width: 100%;
    height: 350rpx;
}

.intro {
    box-sizing: border-box;
    border-bottom: 1rpx solid #e3e3e3;
    padding: 20rpx 40rpx 10rpx 40rpx;
}

.title {
    color: #333;
    font-size: 32rpx;
    line-height: 50rpx;
}

.item-num {
    color: #808080;
    font-size: 26rpx;
}

.tag {
    height: 78rpx;
    padding: 0 40rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #e3e3e3;
}

.tag-item {
    flex: 1;
}

.tag-item-icon {
    height: 36rpx;
    width: 36rpx;
    vertical-align: middle;
    margin-right: 8rpx;
}

.tag-item-text {
    color: #333;
    font-size: 22rpx;
}

.intro-nav {
    position: relative;
    height: 90rpx;
}

.head {
    height: 90rpx;
    padding: 0 72rpx 0 56rpx;
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
    width: 100%;
    border-bottom: 1rpx solid #e3e3e3;
    font-size: 30rpx;
}

.head.fixed {
    position: fixed;
    top: 0;
}

.head-item {
    display: flex;
    align-items: center;
}

.head-item.active .head-item-icon {
    opacity: 1;
}

.head-item.active .head-item-text {
    color: #FD9238;
}

.head-item-icon {
    height: 30rpx;
    width: 30rpx;
    opacity: 0;
}

.head-item-text {
    color: #333;
}

.tag-info {
}

.border-bottom {
    border-bottom: 1rpx solid #e3e3e3;
}

.tag-info-title {
    padding: 50rpx 0 0 0;
    text-align: center;
    color: #808080;
    font-size: 30rpx;
}

.info {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.info-item {
    text-align: center;
    padding: 40rpx 20rpx 60rpx 20rpx;
    box-sizing: border-box;
}

.info-item-img {
    width: 140rpx;
    height: 140rpx;
    margin-bottom: 10rpx;
}

.info-item-title {
    color: #333;
    line-height: 50rpx;
    font-size: 26rpx;
}

.info-item-desc {
    font-size: 24rpx;
    color: #808080;
    line-height: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.margin-bottom {
    margin-bottom: 20rpx;
}

.dentistryDetail image {
    display: block;
    border: none 0;
    width: 100%;
    height: auto;
}

.page-fixed {
    position: fixed;
    width: 100%;
    bottom: 0;
}

.org {
    display: flex;
    justify-content: space-between;
    padding: 0 40rpx;
    color: #333;
    font-size: 28rpx;
    height: 90rpx;
    margin-bottom: 20rpx;
    align-items: center;
}

.arrow_gray_right {
    width: 14rpx;
    height: 26rpx;
}

.org_list {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 6;
}

.transtion_open_class {
    bottom: 0;
    transition: all 0.5s;
}

.transtion_close_class {
    bottom: -780rpx;
    transition: all 0.5s;
}

.mask {
    background-color: rgba(0,0,0,0.4);
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 1;
    outline: 0;
}

.org_list_title {
    height: 100rpx;
    line-height: 100rpx;
    font-size: 36rpx;
    position: relative;
    text-align: center;
    border-bottom: 1rpx solid #e3e3e3;
}

.org_list_title image {
    height: 36rpx;
    width: 36rpx;
    position: absolute;
    top: 33rpx;
    right: 24rpx;
}

.org_list_left {
    width: 200rpx;
    position: absolute;
    left: 0;
    height: 650rpx;
    overflow: auto;
    margin-bottom: 20rpx;
    -webkit-overflow-scrolling: touch;
}

.org_list_left_single {
    height: 88rpx;
    font-size: 30rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background: rgb(245,245,245);
    border-bottom: 1rpx solid #FFFFFF;
}

.select {
    color: rgb(234,85,4);
    background: #ffffff;
}

.org_list_right {
    display: inline-block;
    margin-left: 200rpx;
    height: 650rpx;
    overflow: auto;
    margin-bottom: 20rpx;
    -webkit-overflow-scrolling: touch;
}

.org_list_right_single {
    position: relative;
    padding: 38rpx 35rpx 38rpx 0;
    margin-left: 40rpx;
    border-bottom: 1rpx solid #e3e3e3;
}

.org_list_right_single:last-child {
    border: none;
}

.circle {
    display: inline-block;
    width: 10rpx;
    height: 10rpx;
    border-radius: 10rpx;
    background: #ea5504;
    position: absolute;
    top: 44rpx;
    left: 0;
}

.org_name {
    padding-left: 31rpx;
    font-size: 28rpx;
}

.org_addr {
    padding-left: 31rpx;
    font-size: 26rpx;
    color: rgb(128,128,128);
    margin-top: 19rpx;
}

.checkItemCount {
    text-align: center;
    height: 32px;
    line-height: 32px;
    background-color: #fff8ea;
    color: #ff9f0a;
}

.attention {
    background: #ffffff;
    padding: 0 0 30rpx 0;
}

.attention.margin-bottom {
    padding: 0;
}

.attention.margin-bottom2 {
    margin-bottom: 20rpx;
}

.attention:last-child {
    margin-bottom: 20rpx;
}

.attention-title {
    height: 90rpx;
    padding: 0 20rpx 0;
}

.attention-title-text {
    display: inline-block;
    font-size: 30rpx;
    margin: 20rpx 0;
    padding-left: 24rpx;
    border-radius: 4rpx;
    border-left: 6rpx solid #FD9238;
}

.attention-item {
    padding: 0 20rpx 0;
    color: rgb(128,128,128);
    margin: 0;
    line-height: 1.3;
}

.pl_40 {
    padding-left: 20rpx!important;
}

.check-type-name {
    font-size: 26rpx;
    color: #808080;
    background-color: #f5f5f5;
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
    margin-bottom: 10rpx;
    margin-right: 40rpx;
    padding-right: 40rpx;
}

.check-item-box {
    display: flex;
    justify-content: space-between;
    align-items: center;
    min-height: 128rpx;
    box-sizing: border-box;
    padding: 27rpx 0 20rpx 0;
    border-bottom: 1rpx solid #e3e3e3;
}

.check-item-box:last-child {
    padding: 27rpx 0 30rpx 0;
    border-bottom: 0;
}

.dept {
    text-align: justify;
    width: 570rpx;
}

.dept-type-name {
    color: #333;
    font-size: 28rpx;
    line-height: 48rpx;
    margin-bottom: 12rpx;
}

.dept-type-detail {
    color: #808080;
    font-size: 26rpx;
    line-height: 40rpx;
}

.dept-tip {
    width: 140rpx;
}

.dept-tip-icon {
    margin-left: 64rpx;
    margin-right: 40rpx;
    width: 36rpx;
    height: 36rpx;
}

.noMR {
    margin-right: 0;
}

.bottom {
    height: 112rpx;
    position: relative;
    align-items: center;
}

.bottom-box {
    position: fixed;
    background: #fff;
    width: 100%;
    height: 98rpx;
    bottom: 0;
    border-top: 1rpx solid #e3e3e3;
    text-align: right;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.bottom-text {
    color: #E94835;
    height: 98rpx;
    padding-left: 40rpx;
    box-sizing: border-box;
    text-align: left;
    display: flex;
    align-items: center;
    font-size: 28rpx;
}

.bottom-text-unit {
    font-size: 30rpx;
}

.bottom-text-big {
    font-size: 40rpx;
}

.bottom-btn {
    color: #fff;
    background-color: #0087E0;
    line-height: 98rpx;
    width: 200rpx;
    height: 98rpx;
    text-align: center;
    font-size: 30rpx;
}

.couponListTap {
    margin: 16rpx 0;
    background: #fff;
    height: 96rpx;
    line-height: 96rpx;
    color: #2A2827;
    font-size: 32rpx;
}

.couponListTap .rightArrow {
    float: right;
    width: 12rpx;
    height: 20rpx;
    margin-top: 38rpx;
    margin-right: 32rpx;
}

.couponListTap .icon {
    float: left;
    width: 52rpx;
    height: 30rpx;
    margin-top: 34rpx;
    margin-left: 30rpx;
    margin-right: 12rpx;
}

.couponList {
    width: 100%;
    box-sizing: border-box;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 2000;
    background: #f5f5f5;
    padding-left: 32rpx;
    padding-right: 30rpx;
    overflow: hidden;
}

.couponList .couponListTitle {
    height: 96rpx;
    line-height: 96rpx;
    font-size: 34rpx;
    color: #222222;
}

.couponList .couponListTitle image {
    float: right;
    width: 28rpx;
    height: 28rpx;
    margin-top: 34rpx;
}

.couponList .list {
    position: relative;
    width: 670rpx;
    height: 200rpx;
    margin-bottom: 32rpx;
}

.couponList .list .left {
    float: left;
}

.couponList .list .left.zk,.couponList .list .left.mj {
    width: 210rpx;
    height: 200rpx;
    color: #fff;
    text-align: center;
    text-indent: -15rpx;
    position: relative;
}

.couponList .list .left.zk image,.couponList .list .left.mj image {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    color: #fff;
    text-align: center;
    text-indent: -15rpx;
}

.couponList .list .left.zk .col_1,.couponList .list .left.mj .col_1 {
    font-size: 30rpx;
    margin-top: 60rpx;
    position: absolute;
    left: 0;
    width: 100%;
}

.couponList .list .left.zk .col_1 text,.couponList .list .left.mj .col_1 text {
    font-size: 40rpx;
    margin-left: -15rpx;
}

.couponList .list .left.zk .col_2,.couponList .list .left.mj .col_2 {
    font-size: 26rpx;
    margin-top: 110rpx;
    position: absolute;
    left: 0;
    width: 100%;
}

.couponList .list .left.content {
    width: 460rpx;
    background: #fff;
    height: 200rpx;
    border-top-right-radius: 12rpx;
    border-bottom-right-radius: 12rpx;
    box-shadow: 0 0.5rpx 0.5rpx 0 rgba(0,0,0,.3);
}

.couponList .left.content .couponName {
    margin-top: 22rpx 0 0 24rpx;
    height: 68rpx;
    line-height: 68rpx;
    font-size: 32rpx;
    color: #333;
    overflow: hidden;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.couponList .left.content .getCoupon {
    position: absolute;
    width: 132rpx;
    height: 56rpx;
    line-height: 56rpx;
    text-align: center;
    right: 22rpx;
    bottom: 30rpx;
    font-size: 26rpx;
    color: #FD9238;
    border: 1px solid #FD9238;
    border-radius: 8rpx;
    background: rgba(253,146,56,0.1);
}

.couponList .left.content .couponTip {
    font-size: 24rpx;
    color: #666;
    height: 28rpx;
    line-height: 28rpx;
    margin-bottom: 30rpx;
}

.couponList .left.content .time {
    font-size: 24rpx;
    color: #808080;
    width: 280rpx;
}

.couponList .geted {
    position: absolute;
    right: 0;
    top: 0;
    width: 100rpx;
    height: 100rpx;
}

.mark {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
