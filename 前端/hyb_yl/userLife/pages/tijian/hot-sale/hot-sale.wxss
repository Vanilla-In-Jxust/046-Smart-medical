.package_item_box {
    width: 100%;
    padding: 40rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-around;
}

.package_item_box:last-child {
    border: none;
}

.package_img {
    flex: 0 0 30%;
    height: 160rpx;
    margin-right: 36rpx;
}

.package_right {
    flex: 0 0 70%;
    overflow: hidden;
}

.title {
    font-size: 32rpx;
    color: #494949;
    margin-bottom: 16rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.describle {
    padding-top: 2rpx;
    font-size: 24rpx;
    color: #A3A3A3;
    margin-bottom: 28rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.number {
    display: flex;
    align-items: baseline;
    justify-content: space-between;
}

.price {
    color: #ea5504;
    font-size: 24rpx;
}

.big {
    font-size: 36rpx;
}

.people {
    color: #808080;
    font-size: 24rpx;
}

.package-detail-head {
    margin-bottom: 20rpx;
}

.a-dropdown-wrapper {
    position: relative;
    display: flex;
    flex-direction: column;
    width: 100%;
    height: 90rpx;
    color: #666;
}

.a-dropdown-wrapper.expand {
    position: fixed;
    height: 100%;
    z-index: 99;
}

.a-dropdown-nav {
    display: flex;
    position: relative;
    border-bottom: 1rpx solid #ddd;
    background: #fff;
    min-height: 88rpx;
    z-index: 99;
}

.a-dropdown-nav-item {
    flex: 1;
    padding-left: 40rpx;
    height: 88rpx;
    line-height: 88rpx;
    color: #333333;
}

.a-dropdown-wrapper.expand .a-dropdown-nav-item .triangle {
    border-bottom: 10rpx solid rgb(102,102,102);
    border-top: 0;
}

.triangle {
    width: 0;
    height: 0;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
    border-top: 10rpx solid #333333;
    border-bottom: 0;
    vertical-align: middle;
    display: inline-block;
    margin-left: 14rpx;
}

.a-dropdown-contents {
    z-index: 2;
}

.a-dropdown-list-items {
    height: 0;
    overflow: hidden;
    background: #fff;
    position: relative;
    z-index: 98;
}

.a-dropdown-list-items.show {
    height: auto;
}

.a-dropdown-list-items .a-dropdown-list-item {
    display: flex;
    line-height: 90rpx;
    min-height: 90rpx;
    align-items: center;
    padding-left: 40rpx;
    background: #fff;
}

.a-dropdown-list-item.selected {
    color: #ea5504;
}

.a-dropdown-list-item .a-dropdown-list-item-bottom {
    display: block;
    position: absolute;
    width: 100%;
    left: 0;
    bottom: 0;
    right: auto;
    top: auto;
}

.a-dropdown-list-item .a-dropdown-list-item-thumb {
    width: 44rpx;
    height: 44rpx;
    margin-right: 30rpx;
}

.a-dropdown-list-item .a-dropdown-list-item-line {
    position: relative;
    display: flex;
    flex: 1;
    align-items: center;
    align-self: stretch;
    padding-right: 40rpx;
    min-height: 88rpx;
    overflow: hidden;
}

.a-dropdown-list-item .a-dropdown-list-item-content {
    flex: 1;
    text-align: left;
    line-height: 1.5;
    width: auto;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding-top: .14rem;
    padding-bottom: .14rem;
}

.a-dropdown-list-item.last .a-dropdown-list-item-bottom {
    border-bottom: none;
}

.a-dropdown-list-item .a-dropdown-list-item-line-wrap .a-dropdown-list-item-content {
    white-space: normal;
}

.a-dropdown-list-item.a-dropdown-list-item-hover {
    background-color: #ddd;
}

.a-dropdown-list-item-check {
    display: block;
    width: 35rpx;
    height: 35rpx;
    margin-left: 16rpx;
    background-image: url('data:image/svg+xml;charset=utf-8,<svg t="1500636672907" class="icon" style="" viewBox="0 0 1024 1024" version="1.1" xmlns="http://www.w3.org/2000/svg" p-id="1435" xmlns:xlink="http://www.w3.org/1999/xlink" width="350" height="350"><defs><style type="text/css"></style></defs><path d="M824.66816 299.58144 429.54752 694.70208l-190.32064-190.32064c-8.00768-8.00768-20.95104-8.00768-28.95872 0s-8.00768 20.95104 0 28.95872l204.8 204.8c3.9936 3.9936 9.23648 6.00064 14.47936 6.00064s10.48576-2.00704 14.47936-6.00064l409.6-409.6c8.00768-8.00768 8.00768-20.95104 0-28.95872S832.65536 291.57376 824.66816 299.58144z" p-id="1436" fill="#f76a24"></path></svg>');
    background-size: contain;
    background-repeat: no-repeat;
    background-position: 50% 50%;
    transition: transform 0.3s;
    transition: transform 0.3s,-webkit-transform 0.3s;
    transition: transform 0.3s,-webkit-transform 0.3s;
    visibility: hidden;
}

.a-dropdown-list-item.selected .a-dropdown-list-item-check {
    visibility: visible;
}

.a-dropdown-placeholder {
    z-index: 1;
    flex: 1;
}

.a-dropdown-bg {
    opacity: 0;
    transition: opacity linear 0.1s;
}

.expand .a-dropdown-bg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgb(0,0,0);
    opacity: 0.4;
    z-index: 1;
}

.expand .a-dropdown-list-items {
    height: auto;
}

.grid.fixed {
    min-height: 320rpx;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 100;
}

.block {
    min-height: 326rpx;
}

.grid {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    padding-top: 6rpx;
    background: #fff;
}

.grid-item {
    text-align: center;
    background-color: white;
    position: relative;
    height: 160rpx;
}

.grid-item-wrapper {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 160rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    background: #fff;
}

.grid-border-icon {
    width: 88rpx;
    height: 88rpx;
    position: absolute;
    top: 12rpx;
    display: none;
    left: 50%;
    margin-left: -46rpx;
    border-radius: 100%;
    border: 1px solid #FD9238;
}

.grid-item-wrapper.active .grid-border-icon {
    display: block;
}

.grid-item-wrapper.active .grid-text {
    color: #FD9238;
}

.grid-icon {
    margin-top: 0rpx;
    width: 88rpx;
    height: 88rpx;
    border-radius: 100%;
}

.grid-text {
    color: #494644;
    line-height: 1;
    margin-top: 20rpx;
    font-size: 24rpx;
}

.swiper-tab {
    display: flex;
    flex-direction: row;
    line-height: 91rpx;
    justify-content: center;
    background: #fff;
    height: 96rpx;
    padding-top: 16rpx;
    padding-bottom: 16rpx;
}

.tab-item {
    width: 18.7%;
    text-align: center;
    font-size: 15px;
    color: #494644;
}

.tab-item:first-child {
    margin-left: 0;
}

.swiper-tab-content {
    height: 500rpx;
    width: 100%;
}

.swiper,.swiper swiper {
    height: 250rpx;
}

.imagetop {
    width: 100%;
    height: 250rpx;
