page {
    background: #f2f3f7;
}

.wrap {
    width: 100%;
    height: 104rpx;
    background: #e0e0e0;
    position: fixed;
    top: 0;
    left: 0;
    box-sizing: border-box;
    z-index: 100;
}

.wrap-seet {
    width: 690rpx;
    height: 72rpx;
    background: #fff;
    border-radius: 6rpx;
    margin: 16rpx auto;
    box-sizing: border-box;
}

.seet-img {
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin-left: 290rpx;
    margin-top: 16rpx;
    margin-right: 10rpx;
}

.seet_mg_left {
    margin-left: 20rpx;
}

.input {
    font-size: 32rpx;
    height: 72rpx;
    width: 320rpx;
}

.input_flag {
    width: 500rpx;
}

.bank {
    width: 100%;
    height: 82rpx;
    line-height: 82rpx;
    font-size: 32rpx;
    text-indent: 30rpx;
    color: #666;
    background: #f2f3f7;
}

.for {
    width: 100%;
    height: 88rpx;
    background: #fff;
    border-bottom: 1rpx solid #e0e0e0;
}

.forImgBox {
    width: 100rpx;
    height: 88rpx;
    float: left;
}

.forImg {
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin-left: 30rpx;
    margin-top: 24rpx;
    border-radius: 40rpx;
}

.bankname {
    float: left;
    line-height: 88rpx;
    color: #333;
    font-size: 32rpx;
}