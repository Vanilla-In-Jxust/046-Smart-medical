page {
    background: #f5f5f7;
}

.box {
    width: 100%;
    height: 20rpx;
}

.li {
    width: 100%;
    height: 98rpx;
    background: #fff;
    border-bottom: 1rpx solid #f0f0f0;
    line-height: 98rpx;
    text-indent: 30rpx;
    color: #333;
    font-size: 30rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}