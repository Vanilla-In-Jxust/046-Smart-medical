.uploadimg {
    width: 100%;
    margin-top: 20rpx;
    background: #fff;
    padding: 0 30rpx;
    clear: both;
}

.uploadimg-tit {
    font-size: 30rpx;
    height: 100rpx;
    position: relative;
}

.uploadimg-tit text {
    width: 240rpx;
    box-sizing: border-box;
    background: linear-gradient(to right,#6699ff,#3b4ac9);
    color: #fff;
    padding: 10rpx 60rpx 10rpx 30rpx;
    border-top-right-radius: 50rpx;
    border-bottom-right-radius: 50rpx;
    position: absolute;
    top: 50%;
    left: -30rpx;
    transform: translateY(-50%);
}

.uploadimg-gorup {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 30rpx;
    padding: 20rpx;
}

.uploadimg-gorup input {
    color: #999;
    width: 450rpx;
    text-align: right;
    padding: 10rpx 15rpx;
    border: 1rpx solid #f0f0f0;
}

.uploadimg-gorup-imgs {
    width: 490rpx;
}

.uploadimg-gorup-imgs-list {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.uploadimg-gorup-imgs-list view {
    width: 48%;
    text-align: center;
    font-size: 22rpx;
    margin-bottom: 25rpx;
}

.uploadimg-gorup-imgs-list view image {
    width: 100%;
    height: 170rpx;
    margin-bottom: 10rpx;
}