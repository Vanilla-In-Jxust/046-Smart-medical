                    <view class="doctor-star">{{item.score==0?'5':item.score}}</view>
                    <view class="answer-count">月回答{{item.xn_reoly}}</view>
                    <view class="prescription-count"> 月处方{{item.xn_cf}} </view>
                    <view class="reply-time-str">{{item.xytime}}响应</view>
                </view>
                <view class="doctor-list_dis">
                    <view class="doctor-list_dis_tags">过敏类精选医生</view>
                    <view class="doctor-list_dis_tags">业内专家</view>
                    <view class="doctor-list_dis_tags">专业审核优秀</view>
                </view>
                <view class="ask-doctor">
                    <view class="price-values-wrapper">
                        <view class="price-values" wx:if="{{!item.server}}">
                            <view class="price-value">
                                <text>{{tit}}￥{{item.money}}元起</text>
                            </view>
                        </view>
                        <view class="price-values" wx:if="{{item.server.length!=='0'}}">
                            <view class="price-value" wx:if="{{index<2}}" wx:for="{{item.server}}" wx:for-item="itemName">
                                <text>{{itemName.titlme}}￥{{itemName.ptmoney}}</text>
                            </view>
                        </view>
                        <view class="price-values" style="margin-left: 20rpx;" wx:else>
                            <view class="price-value">
                                <text>费用：￥{{item.money}}</text>
                            </view>
                        </view>
                    </view>
                    <view class="ask-doctor-hot">{{tit}}</view>
                </view>
            </view>
        </view>
    </view>
</view>
