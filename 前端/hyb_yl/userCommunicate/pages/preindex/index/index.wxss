page {
    background: #f6f6f6;
    width: 100%;
    height: 100%;
    margin-bottom: 103rpx;
}

.headbox {
    padding: 0rpx 20rpx;
    margin-top: 20rpx;
}

.one {
    background: #FFFFFF;
    border-radius: 10rpx;
}

.top {
    height: 80rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #eaeaea;
    background: #FFFFFF;
    padding: 0rpx 20rpx;
}

.top text {
    font-size: 24rpx;
    color: #333;
}

.top text:nth-of-type(3) {
    color: #4aa0ed;
}

.top image {
    width: 30rpx;
    height: 20rpx;
}

.one_Center {
    padding: 0rpx 20rpx;
}

.makeTitle {
    padding: 30rpx 0rpx;
    border-bottom: 1rpx solid #eaeaea;
}

.oneCont_list {
    padding: 20rpx 0;
    color: #0486dc;
}

.listImg {
    width: 40rpx;
    height: 40rpx;
}

.serverflow_Title {
    padding-left: 20rpx;
    border-left: 6rpx solid #0486dc;
    margin-bottom: 20rpx;
    font-size: 30rpx;
}

.flowbox {
    margin: 30rpx 0rpx;
}

.flowlist {
    position: relative;
    padding-left: 15rpx;
}

.flowImg {
    position: absolute;
    left: -8rpx;
    top: -15rpx;
    width: 50rpx;
    height: 50rpx;
}

.flowCont {
    padding-bottom: 40rpx;
    border-left: 5rpx solid #0486dc;
}

.two {
    width: 100%;
    padding: 0px 20rpx;
    margin-top: 30rpx;
    background: white;
}

.two>text {
    color: #333;
    font-weight: bold;
    font-size: 32rpx;
}

.two .two_con {
    width: 100%;
    height: 200rpx;
    margin-top: 20rpx;
    display: flex;
}

.two .two_con .left {
    width: 50rpx;
    height: 100%;
    position: relative;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
}

.two .two_con .left .shu {
    width: 4rpx;
    height: 90%;
    background: #aed8ff;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}

.two .two_con .left image {
    width: 40rpx;
    height: 40rpx;
}

.two .two_con .right {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: flex-start;
}

.two .two_con .right text {
    color: #666;
    font-size: 28rpx;
}

.three {
    width: 100%;
    padding: 0px 20rpx;
    padding-top: 30rpx;
    background: white;
}

.three>text {
    color: #333;
    font-weight: bold;
    font-size: 32rpx;
}

.three .three_con {
    width: 98%;
    height: auto;
    padding: 20rpx;
    background: #f0f0f0;
    margin: auto;
    margin-top: 50rpx;
    box-shadow: 0px 0px 200rpx #f0f0f0 inset;
    border-radius: 10rpx;
}

.three .three_con text:nth-of-type(1) {
    font-size: 30rpx;
    text-align: center;
}

.three .three_con text {
    color: #666;
    font-size: 28rpx;
    display: block;
}

.four {
    width: 100%;
    height: 80rpx;
    display: flex;
    align-items: center;
}

.four label {
    font-size: 30rpx;
    color: #4aa0ed;
    display: flex;
    align-items: center;
}

.four radio {
    transform: scale(0.7);
}

radio .wx-radio-input.wx-radio-input-checked {
    border: solid 1px transparent;
    background: #4aa0ed;
}

.posview {
    width: 100%;
    height: 100rpx;
    position: fixed;
    bottom: 0px;
    background: white;
    border-top: solid 4rpx #eaeaea;
    text-align: center;
    background: #0486dc;
    color: #FFFFFF;
    line-height: 100rpx;
}

.posview view {
    background: #4aa0ed;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    color: white;
    width: 250rpx;
    height: 70rpx;
    display: flex;
    align-items: center;
    justify-content: space-around;
    font-size: 28rpx;
    border-radius: 8rpx;
}

.mask {
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.5);
    position: fixed;
    top: 0px;
    left: 0px;
}

.mask .mask_con {
    width: 80%;
    height: 60%;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    background: white;
    border-radius: 20rpx;
    box-shadow: 0px 0px 8rpx white;
}

.mask .mask_con .maskc_up {
    width: 100%;
    height: 85%;
    padding: 24rpx;
}

.mask .mask_con .maskc_up text:nth-of-type(1) {
    font-size: 38rpx;
    display: block;
    text-align: center;
    color: #333;
    margin: 20rpx 0px;
}

.mask .mask_con .maskc_up text {
    display: block;
    color: #666;
    font-size: 32rpx;
    line-height: 50rpx;
    margin: 10rpx 0px;
}

.mask .mask_con .maskc_down {
    width: 100%;
    height: 15%;
    display: flex;
    border-top: solid 1px rgb(226,223,223);
}

.mask .mask_con .maskc_down text {
    margin: auto;
    color: #4aa0ed;
    font-size: 38rpx;
}

rich-text {
    width: 100%;
    color: #666;
    text-align: left;
}