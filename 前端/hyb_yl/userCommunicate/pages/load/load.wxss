view {
    box-sizing: border-box;
}

page {
    background: #fafafa;
}

.info {
    width: 100%;
    background: #fff;
    padding: 50rpx 20rpx 20rpx;
    margin-bottom: 20rpx;
}

.drupmes-img {
    width: 150rpx;
    height: 150rpx;
    border: 1rpx solid #FAFAFA;
    text-align: center;
}

.drupmes-img image {
    object-fit: contain;
}

.drupmesbox {
    width: 75%;
}

.son {
    margin-bottom: 8rpx;
}

.subtit {
    font-size: 28rpx;
    color: #ACACAC;
    line-height: 50rpx;
}

.info .top {
    width: 100%;
    display: flex;
    margin-bottom: 10rpx;
    justify-content: space-between;
}

.info .top .left {
    width: 70%;
    height: 100%;
}

.info .left .son {
    width: 100%;
    height: 60rpx;
    display: flex;
    align-items: center;
    font-size: 26rpx;
    font-weight: 600;
}

.info .top .right {
    width: 30%;
    display: flex;
    justify-content: center;
}

.info .right .btn {
    width: 180rpx;
    height: 70rpx;
    padding: 14rpx 20rpx;
    border-radius: 50rpx;
    font-size: 30rpx;
    border: 1rpx solid #848484;
    display: flex;
    justify-content: center;
    align-items: center;
}

.info .tips {
    width: 100%;
    display: flex;
}

.info .tips .image {
    padding-top: 10rpx;
}

.info .tips image {
    display: block;
    width: 30rpx;
    height: 30rpx;
    margin-right: 15rpx;
}

.info .tips .tishi {
    width: 100%;
    font-size: 24rpx;
    color: #d12a3a;
    font-weight: 500;
}

.load {
    width: 100%;
    background: #fff;
}

