    color: #999;
    font-size: 26rpx;
    padding-left: 10rpx;
    box-sizing: border-box;
}

.specialExplainTitleChart {
    width: 30rpx;
    height: 30rpx;
    vertical-align: middle;
}

.servicePackage {
    width: 100%;
    height: 800rpx;
    background: #fff;
    overflow: scroll;
}

.distance {
    display: flex;
    align-items: center;
    padding-right: 20rpx;
    box-sizing: border-box;
}

.distance image {
    width: 30rpx;
    height: 30rpx;
}

.distance text {
    font-size: 24rpx;
    padding-left: 10rpx;
    box-sizing: border-box;
}