    font-size: 26rpx;
    color: #333;
}

.searchresult {
    width: 100vw;
    height: 100vh;
    background: #fff;
    box-sizing: border-box;
    padding: 0 25rpx;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 20;
    padding-top: 96rpx;
}

.searchresultitems {
    font-size: 28rpx;
    color: #333;
    line-height: 92rpx;
    border-bottom: 1rpx solid #EBEBEB;
}

.searchresult {
    width: 100vw;
    height: 100vh;
    background: #fff;
    box-sizing: border-box;
    padding: 0 25rpx;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 20;
    padding-top: 96rpx;
    overflow: hidden;
    overflow-y: auto;
}

.searchall-tab {
    position: fixed;
    top: 100rpx;
    left: 0;
    display: flex;
    justify-content: space-around;
    align-items: center;
    width: 100%;
    height: 100rpx;
    background: #fff;
    border-bottom: 1rpx solid #F0F0F0;
    z-index: 21;
}

.searchtab-item {
    position: relative;
    width: 25%;
    height: 100rpx;
    text-align: center;
    line-height: 100rpx;
    font-size: 28rpx;
}

.tabact::after {
    content: '';
    position: absolute;
    bottom: 0rpx;
    left: 0;
    right: 0;
    margin: 0 auto;
    width: 40rpx;
    height: 4rpx;
    background: #0088DE;
    color: #0088DE;
    border-radius: 6rpx;
}

.tabact {
    color: #0088DE;
}

.search-content {
    height: calc(100vh - 200rpx);
    margin-top: 100rpx;
    overflow-y: auto;
}

.consul-box {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    background: rgba(255,255,255);
    z-index: 998;
}

.consul-search {
    width: 690rpx;
    margin: 20rpx 30rpx;
    padding: 18rpx 25rpx;
    border: 1rpx solid #D2D2D2;
    border-radius: 50rpx;
    display: inline-flex;
    align-items: center;
    font-size: 30rpx;
    color: #D2D2D2;
}

.consul-search image {
    width: 35rpx;
    height: 35rpx;
    margin-right: 20rpx;
}

.bar {
    width: 100%;
    height: 110rpx;
}

.reward {
    width: 690rpx;
