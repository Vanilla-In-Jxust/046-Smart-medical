    align-items: center;
    justify-content: space-around;
}

.searchtypesitem {
    text-align: center;
}

.searchtypesitem image {
    width: 65rpx;
    height: 65rpx;
}

.searchtypesitem view {
    font-size: 30rpx;
    color: #848484;
    margin-top: 8rpx;
}

.all {
    padding: 0rpx 20rpx;
