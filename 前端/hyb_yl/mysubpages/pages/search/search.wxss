}

.historyList {
    padding: 12rpx 25rpx;
    border-radius: 10rpx;
    background: #f7f7f7;
    color: #222;
    font-size: 24rpx;
    margin: 0rpx 20rpx 20rpx 0rpx;
    display: inline-flex;
    align-items: center;
}

.historyList image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 8rpx;
}

.findbox {
    display: flex;
    flex-wrap: wrap;
    padding: 0rpx 30rpx;
}

.findList {
