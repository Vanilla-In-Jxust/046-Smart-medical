    color: #727272;
}

.reward-dl {
    font-size: 26rpx;
    color: #0088DE;
}

.reward-list {
    position: relative;
    margin-top: 20rpx;
    width: 100%;
}

.reward-bar {
    position: absolute;
    top: 50%;
    left: 0;
    width: 630rpx;
    margin: 0 30rpx;
    height: 2rpx;
    background: #EBEBEB;
    z-index: 990;
}

.reward-day-box {
    position: relative;
    display: flex;
    justify-content: space-around;
    align-items: center;
    height: 40rpx;
    z-index: 991;
}

.reward-day {
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
