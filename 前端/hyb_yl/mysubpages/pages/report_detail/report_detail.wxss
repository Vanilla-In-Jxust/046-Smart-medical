page {
    box-sizing: border-box;
    background: #fff;
}

view {
    box-sizing: border-box;
}

.elsemess {
    padding: 25rpx 25rpx;
}

.sickitem {
    box-shadow: 0 0 5rpx 5rpx #FAFAFA;
    border-radius: 10rpx;
    margin-bottom: 26rpx;
    width: 100%;
    display: flex;
    align-items: stretch;
    justify-content: space-between;
    overflow: hidden;
}

.sickitem-left {
    width: 32%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 20rpx;
}

.sickitem-left image {
    width: 70rpx;
    height: 70rpx;
}

.sickiteminfo {
    width: 55%;
}

.sickiteminfo view:first-child {
    font-size: 30rpx;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.sickiteminfo view:last-child {
    font-size: 26rpx;
    color: #A7A7A7;
    margin-top: 8rpx;
}

.sickitem-center {
    width: 65%;
    padding: 0 20rpx;
    border-left: 1rpx solid #F8F8F8;
    margin: 20rpx 0;
}

.sickitem-center view {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.sickitem-center view:first-child {
    font-size: 30rpx;
    color: #333;
}

.sickitem-center view:nth-child(2) {
    font-size: 24rpx;
    color: #8C8C8C;
    margin: 8rpx 0;
}

.sickitem-center view:last-child {
    font-size: 22rpx;
    color: #8C8C8C;
}

.sickbtn {
    width: 66rpx;
    font-size: 26rpx;
    color: #fff;
    text-align: center;
    padding: 10rpx;
}

view.btn1 {
    background: #2876E6;
}

view.btn2 {
    background: #E67428;
}

view.btn3 {
    background: #8E6EF9;
}

.servepackage {
    width: 100%;
    display: flex;
    justify-content: space-between;
    padding: 30rpx 0;
    border-bottom: 1rpx solid #F9F9F9;
}

.servepackage image {
    width: 85rpx;
    height: 85rpx;
    border-radius: 10rpx;
}

.servepackageinfo {
    width: 85%;
}

.info-top {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.info-top view:first-child {
    color: #454545;
    font-size: 32rpx;
}

.info-top view:first-child text:first-child {
    overflow: hidden;
    color: #333;
    font-weight: 500;
    font-size: 34rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.info-top view:first-child text:last-child {
    margin: 0 12rpx;
    overflow: hidden;
    color: #333;
    font-size: 28rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.info-top view:last-child {
    color: #E72D2D;
    font-size: 34rpx;
}

.info-bott {
    color: #333;
    font-weight: 300;
    font-size: 28rpx;
    line-height: 40rpx;
}

.tags {
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    height: 36rpx;
    overflow-y: hidden;
}

.tag {
    padding: 4rpx;
    color: #fa0;
    font-size: 24rpx;
    line-height: 28rpx;
    background-color: #fffaf0;
    border-radius: 4rpx;
}

.elsegroup {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    color: #525252;
    font-size: 32rpx;
    border-bottom: 1rpx solid #F9F9F9;
    padding: 30rpx 0;
}

.youhui {
    position: relative;
    padding-right: 25rpx;
}

.elsegroup view:last-child {
    color: #333;
    font-weight: 300;
    font-size: 28rpx;
}

.youhui view:last-child::after {
    content: "";
    width: 15rpx;
    height: 15rpx;
    border-top: 1rpx solid #D6D6D6;
    border-right: 1rpx solid #D6D6D6;
    position: absolute;
    top: 50%;
    right: 0;
    transform: translateY(-50%) rotate(45deg);
}

.package {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 28rpx;
    background: #fff;
    background-repeat: no-repeat;
    background-size: 100% 100%;
}

.package {
    margin-top: 30rpx;
}

.package,.package-body {
    display: flex;
    align-items: center;
}

.package-body-image {
    width: 64rpx;
    height: 64rpx;
    margin-right: 12rpx;
}

.package-body-content-title {
    display: flex;
    align-items: center;
    color: #333;
    font-weight: 500;
    font-size: 28rpx;
    line-height: 32rpx;
}

.package-body-content-detail {
    display: flex;
    align-items: center;
    max-width: 500rpx;
    margin-top: 8rpx;
}

.package-body-content-detail-text {
    color: #666;
    font-size: 22rpx;
    line-height: 26rpx;
}

.package-body-content-detail-img {
    width: 26rpx;
    height: 26rpx;
    margin-left: 8rpx;
}

.package-body-content-detail-img .image {
    display: block;
    width: 26rpx;
    height: 26rpx;
}

.package-foot {
    display: flex;
    align-items: center;
    justify-content: center;
}

.package .price-vertical .package-foot-price {
    display: flex;
    flex-flow: column nowrap;
    align-items: center;
    justify-content: center;
}

.package .price-vertical .package-foot-price-now {
    color: #b38c3e;
    font-weight: 500;
    font-size: 34rpx;
    line-height: 38rpx;
}

.package .price-vertical .package-foot-price-origin {
    margin-top: 4rpx;
    color: #666;
    font-size: 22rpx;
    line-height: 26rpx;
}

.package-foot-filter {
    display: flex;
    align-items: center;
    margin-top: -2rpx;
    transform: scale(.7);
}

.package-foot-filter .radio {
    width: 40rpx;
    margin-left: 24rpx;
}

.footpay {
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    background: #fff;
}

.radios {
    font-size: 32rpx;
    color: #7F7F7F;
    text-align: left;
    margin-bottom: 25rpx;
}

.radios radio {
    transform: scale(0.8);
    margin-left: -25rpx;
}

.payandtotal {
    padding: 15rpx 25rpx;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-top: 1rpx solid #F9F9F9;
}

.payandtotal view:first-child text:first-child {
    font-size: 26rpx;
    color: #989898;
}

.payandtotal view:first-child text:last-child {
    font-size: 32rpx;
    color: #E72D2D;
}

.payandtotal view:last-child {
    width: 55%;
    text-align: center;
    line-height: 85rpx;
    font-size: 30rpx;
    color: #fff;
    background: #0087E0;
    border-radius: 50rpx;
}