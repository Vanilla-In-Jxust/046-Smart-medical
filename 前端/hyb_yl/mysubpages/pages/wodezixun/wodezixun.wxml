                        <view class="list">
                            <view class="list_left">订单号:</view>
                            <view class="list_right">{{item.orders}}</view>
                        </view>
                    </view>
                    <block wx:if="{{item.keywords=='dianhuajizhen'}}">
                        <view catchtap="gettel" class="spbtn" data-ifpay="{{item.ifpay}}" wx:if="{{item.ifpay==2}}">去电话</view>
                    </block>
                    <block wx:if="{{item.keywords=='shipinwenzhen'}}">
                        <view catchtap="joinRoom" class="spbtn" data-ifpay="{{item.ifpay}}" wx:if="{{item.ifpay==2}}">去视频</view>
                    </block>
                </view>
                <view class="time">{{item.time}} </view>
            </view>
        </view>
    </view>
    <view class="nonelist" style="padding-top:120rpx;" wx:else>
        <image src="https://6a69-jin5201-a4503e-1257013711.tcb.qcloud.la/nodata.png?sign=705a440d11d3215a89b242147c3fe256&t=1576576018"></image>
        <view class="fs28">暂未咨询记录</view>
    </view>
</view>
