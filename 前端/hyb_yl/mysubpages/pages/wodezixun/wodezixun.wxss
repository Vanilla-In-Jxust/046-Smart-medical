page {
    background: #FAFAFA;
}

.topTabSwiper {
    width: 100%;
    display: flex;
    line-height: 90rpx;
    border-bottom: 1px solid #ccc;
    background: white;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100;
}

.tab {
    flex: 1;
    text-align: center;
    position: relative;
}

.topTabSwiper:after {
    content: "";
    clear: both;
    display: block;
}

.tabBorer {
    border-bottom: 2rpx solid #4281f2;
    color: #378dd8;
}

.actives {
    color: #378dd8;
}

.actives::after {
    content: "";
    width: 40%;
    border-bottom: 2rpx solid #4281f2;
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
}

.successbox {
    margin-top: 115rpx;
}

.nonelist {
    text-align: center;
    padding-top: 200rpx;
}

.nonelist image {
    margin-bottom: 20rpx;
    width: 510rpx;
    height: 230rpx;
}

.nosuccess {
    background: white;
    margin-top: 20rpx;
    padding: 0 25rpx;
}

.expertbox {
    font-size: 28rpx;
    display: flex;
    align-items: center;
    padding: 20rpx 0;
    border-bottom: 1rpx solid #f1f1f1;
}

.expertbox>image {
    width: 55rpx;
    height: 55rpx;
    margin-right: 15rpx;
    border-radius: 50%;
}

.inquiry {
    color: #8A8A8A;
    font-weight: bold;
    margin-right: 15rpx;
}

.price {
    margin-left: 30rpx;
    color: #fec67c;
    display: flex;
    font-size: 28rpx;
}

.consult {
    flex: 1;
    text-align: right;
    font-size: 26rpx;
    color: #F5CB7B;
}

.patientList {
    padding: 5rpx 0;
    font-size: 28rpx;
}

.list {
    display: flex;
    width: 100%;
    line-height: 60rpx;
}

.list_left {
    color: #757575;
}

.list_right {
    color: #757575;
    padding-left: 20rpx;
    flex: 1;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.time {
    padding-bottom: 20rpx;
    font-size: 24rpx;
    color: #cbcbcb;
}

.time text {
    padding: 5rpx 10rpx;
    border-radius: 40rpx;
    border: 1rpx solid #4281f2;
    color: #4281f2;
    font-size: 28rpx;
    margin-left: 20rpx;
}

.spbtn {
    background: #4281f2;
    color: #fff;
    font-size: 26rpx;
    padding: 0 20rpx;
    border-radius: 10rpx;
    height: 66rpx;
    line-height: 66rpx;
    white-space: nowrap;
    margin-left: 20rpx;
}

.dfbox {
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
}