page {
    width: 100%;
    height: 100%;
    background: #fff;
}

view,text {
    box-sizing: border-box;
}

.infobox {
    border-bottom: 25rpx solid #F6F6F6;
}

.infobj {
    width: 100%;
    height: 450rpx;
    position: relative;
}

.infobj image.bjimg {
    width: 100%;
    height: 100%;
}

.infobj image.saoyisao {
    width: 45rpx;
    height: 45rpx;
    position: absolute;
    top: 25rpx;
    right: 25rpx;
    z-index: 20;
}

.infomes {
    width: 59%;
    position: absolute;
    background: #FAFBFE;
    left: 37%;
    top: 25%;
    z-index: 10;
    padding: 30rpx;
    border-radius: 10rpx;
}

.infomes::after {
    content: "";
    width: 0;
    height: 0;
    border-width: 16rpx;
    border-style: solid;
    border-color: transparent #FAFBFF transparent transparent;
    position: absolute;
    top: 50%;
    left: -30rpx;
    transform: translateY(-50%);
}

.names {
    font-size: 32rpx;
    color: #333;
}

.mextext {
    font-size: 24rpx;
    color: #707071;
    margin-top: 8rpx;
}

.jianyibox {
    width: 100%;
    padding: 25rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding-top: 35rpx;
}

.jianyibox image {
    width: 85rpx;
    height: 85rpx;
}

.jianyibox .jianyimes {
    width: 85%;
    position: relative;
}

.jianyibox .jianyimes::after {
    content: "";
    width: 16rpx;
    height: 16rpx;
    border-top: 1rpx solid #A6A6A6;
    border-right: 1rpx solid #A6A6A6;
    position: absolute;
    top: 50%;
    right: 0;
    transform: translateY(-50%) rotate(45deg);
}

.jianyibox .jianyimes view:first-child {
    font-size: 30rpx;
    color: #333;
}

.jianyibox .jianyimes view:last-child {
    font-size: 26rpx;
    color: #ADADAD;
    margin-top: 8rpx;
}

.resultbox {
    width: 100%;
    padding: 0 25rpx;
}

.retulttit {
    font-size: 32rpx;
    color: #333;
    position: relative;
    padding: 30rpx 0;
    padding-left: 25rpx;
}

.retulttit::after {
    content: "";
    width: 5rpx;
    height: 30%;
    background: #004FDA;
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
}

.retuleitem {
    border-top: 1rpx solid #EAEAEA;
    padding-bottom: 30rpx;
}

.rlitemtit {
    line-height: 90rpx;
    width: 100%;
    font-size: 30rpx;
    color: #414141;
    position: relative;
}

.rlitemtit::after {
    content: "";
    width: 16rpx;
    height: 16rpx;
    border-top: 1rpx solid #A6A6A6;
    border-right: 1rpx solid #A6A6A6;
    position: absolute;
    top: 50%;
    right: 0;
    transform: translateY(-50%) rotate(45deg);
}

.rlitemmes {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 26rpx;
    margin-top: 15rpx;
}

.retuleitem view:nth-child(2) {
    margin-top: 0;
}

.rlitemmes view:first-child {
    color: #ADADAD;
}

.rlitemmes view.true {
    color: #E23A3A;
}

.rlitemmes view.false {
    color: #5995EC;
}

.tijianbtn {
    width: 80%;
    background: #0087E0;
    margin: 50rpx auto;
    border-radius: 50rpx;
    font-size: 32rpx;
    color: #fff;
    text-align: center;
    line-height: 95rpx;
}

.jiedubox {
    background: #F6F6F6;
    padding-bottom: 10rpx;
    padding-top: 5rpx;
}

.eqcordimg {
    width: 100%;
    margin: 100rpx auto;
    text-align: center;
    font-size: 22rpx;
    color: #ADADAD;
}

.eqcordimg image {
    width: 300rpx;
    height: 300rpx;
    display: block;
    margin: 0 auto 10rpx;
}

.profesitem {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    border-top: 1rpx solid #F7F7F7;
    padding: 30rpx 0;
}

.profesitem-headimg {
    width: 65rpx;
    height: 65rpx;
    border-radius: 50%;
    overflow: hidden;
    border: 1rpx solid #F0F0F0;
}

.profesitem-mess {
    width: 88%;
}

.proname {
    font-size: 32rpx;
    color: #2B2B2B;
}

.prosite {
    font-size: 26rpx;
    color: #B7B7B7;
}

.promess {
    font-size: 28rpx;
    color: #707070;
    line-height: 45rpx;
}

.shibie {
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #F6F6F6;
    padding: 30rpx;
}

.shibie image {
    width: 120rpx;
    height: 120rpx;
    margin-right: 30rpx;
}

.shibietext view:first-child {
    font-size: 32rpx;
    color: #5B5B5B;
}

.shibietext view:last-child {
    font-size: 26rpx;
    color: #5B5B5B;
    margin-top: 8rpx;
}

.jianyicont {
    width: 100%;
    background: #fff;
    padding: 24rpx;
    box-sizing: border-box;
    border-top: 2rpx solid #EAEAEA;
    font-size: 30rpx;
    color: #999;
}

.imgbox {
    width: 100%;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
}

.imgbox image {
    width: 25%;
    margin-right: 85rpx;
    margin-bottom: 30rpx;
}

.imgbox image:nth-child(3n) {
    margin-right: 0;
}

.jianyitext {
    margin-bottom: 30rpx;
}

.jianyishow .jianyimes::after {
    content: "";
    width: 16rpx;
    height: 16rpx;
    border-top: 1rpx solid transparent;
    border-bottom: 1rpx solid #A6A6A6;
    border-right: 1rpx solid #A6A6A6;
    position: absolute;
    top: 50%;
    right: 0;
    transform: translateY(-50%) rotate(45deg);
}

.rlitshow::after {
    content: "";
    width: 16rpx;
    height: 16rpx;
    border-top: 1rpx solid transparent;
    border-bottom: 1rpx solid #A6A6A6;
    border-right: 1rpx solid #A6A6A6;
    position: absolute;
    top: 50%;
    right: 0;
    transform: translateY(-50%) rotate(45deg);
}