<view class="oval">
    <view class="searchbox">
        <view class="searchView">
            <icon class="searchbtn" color="#999999" size="15" type="search"></icon>
            <input bindconfirm="getSearcharr" bindinput="getSearch" placeholder="搜索医院" type="text" value="{{searchCont}}"></input>
        </view>
    </view>
</view>
<view class="all">
    <view class="searchHistory">
        <view class="history_title">
            <view class="searchFont">热门搜索</view>
            <view class="clearFont"></view>
        </view>
        <view class="historybox">
            <view bindtap="searchHistory" class="historyList" data-item="{{item.keywords}}" wx:for="{{findArr}}" wx:key="*this">
                <text>{{item.keywords}}</text>
            </view>
        </view>
    </view>
</view>
<view class="searchresult" wx:if="{{searchCont!=''}}">
    <view bindtap="listitembtn" class="searchresultitem" data-name="{{item.sname}}" data-sid="{{item.sid}}" wx:for="{{searchlist}}" wx:key="f">
        <view class="listitem">
            <view>{{item.sname}}</view>
            <view>{{item.hospi.agentname}}</view>
        </view>
        <view class="listitem-grade">{{item.hospi.level_name?item.hospi.level_name:''}}</view>
    </view>
</view>
