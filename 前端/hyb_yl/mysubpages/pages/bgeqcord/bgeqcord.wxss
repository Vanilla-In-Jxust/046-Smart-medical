page {
    width: 100%;
    height: 100%;
    background: #fff;
}

view,text {
    box-sizing: border-box;
}

image {
    width: 100%;
    height: 100%;
}

.successbox {
    width: 100%;
    height: 360rpx;
    position: relative;
}

.successbox view {
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    font-size: 32rpx;
    color: #fff;
    line-height: 140rpx;
    text-align: center;
}

.eqcordimg {
    width: 300rpx;
    height: 300rpx;
    margin: 100rpx auto;
}

.tijianbtn {
    width: 80%;
    background: #0087E0;
    margin: 50rpx auto;
    border-radius: 50rpx;
    font-size: 32rpx;
    color: #fff;
    text-align: center;
    line-height: 95rpx;
    margin-top: 200rpx;
}