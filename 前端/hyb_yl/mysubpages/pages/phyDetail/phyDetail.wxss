}

.Amounts {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 30rpx;
    padding-top: 30rpx;
}

.Amouts_left {
    color: #363636;
}

.Amouts_right {
    color: #484848;
}

.sum_Amounts {
    padding: 50rpx 0rpx 30rpx;
    display: flex;
    justify-content: space-between;
}

.sum_price {
    color: #ea7734;
    font-size: 30rpx;
    padding-left: 10rpx;
}

.footbox {
    width: 100%;
    height: 100rpx;
    background: white;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    display: flex;
    justify-content: space-between;
    align-items: stretch;
    padding-left: 30rpx;
}

.footbox2 {
    width: 100%;
    height: 100rpx;
