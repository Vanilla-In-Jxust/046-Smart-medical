}

.mapsgrop {
    padding-bottom: 26rpx;
}

.orderListmaps image {
    width: 46rpx;
    height: 46rpx;
}

.mealImg {
    width: 30rpx;
    height: 30rpx;
    margin-right: 20rpx;
}

.mealCont_title {
    border-bottom: 1rpx solid #f5f4f9;
    display: flex;
    color: #333;
    font-size: 26rpx;
    padding-bottom: 25rpx;
}

.tcTop {
    padding: 25rpx 0rpx;
    display: flex;
    align-items: center;
}

.tcTitle {
    font-size: 38rpx;
    color: #333;
    font-weight: bold;
    margin-right: 30px;
}

.tongy {
