            <view class="informationRightText" style="color: #999;" wx:if="{{item.status==1&&item.nonReadCount==0}}">已完成</view>
        </view>
    </navigator>
    <view class="loading_view" hidden="{{loadingBoo}}">
        <text>加载中...</text>
