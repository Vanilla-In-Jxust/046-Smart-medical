<view class="cancel_view" wx:if="{{Wshow}}">
    <view class="cancel_in">
        <view class="font_si18 font_co333 cancel_in_title">解约确认</view>
        <view class="cancel_content">
            <view class="font_si14 font_co333 cancel_content_in_top">解约原因</view>
            <view class="cancel_content_in font_">
                <view bindtap="menuClick" class="cancel_text {{_num==index?'cancel_active':''}}" data-num="{{index}}" data-text="{{item}}" data-uuid="{{textID[index]}}" wx:for="{{text}}" wx:key="{{ index }}">{{item}}</view>
            </view>
        </view>
        <view class="cancel_bottom">
            <view bindtap="call" class="cancel_bottom_left">我再想想</view>
            <view bindtap="wzconfirm" class="cancel_bottom_right" data-q_id="{{q_id}}">解约</view>
        </view>
    </view>
</view>
<view class="WZwrap-top">
    <view class="WZwrap-left">
        <view class="WZwrap-position"></view>
        <view class="WZwrap-leftBox">
            <view class="WZwrap-leftBox1">
                <view class="WZwrap-leftBox1a"></view>
                <view class="WZwrap-leftBox1b"></view>
            </view>
            <view class="WZwrap-leftBox2">共计{{signCount}}患者</view>
        </view>
    </view>
    <view class="WZwrap-right">
        <view bindtap="seet" class="WZwrap-rightSeet">
            <image class="WZwrap-Seetimg" src="https://6a69-jin5201-a4503e-1257013711.tcb.qcloud.la/ss.png?sign=6e9f2c73747cadbf393bb278344b1096&t=1571646559"></image>
            <text class="">患者名</text>
        </view>
    </view>
</view>
<view class="WZwrap-headline">
  分组
  <view bindtap="Wmodalinput" class="WZwrap-headlineNew">
        <text>+</text> 新建</view>
</view>
<view class="WZwrap-forbox">
    <view bindlongpress="longTap" bindtouchend="bindTouchEnd" bindtouchstart="bindTouchStart" catchtap="ClickGrouping" class="WZwrap-for" data-fenzuid="{{item.fenzuid}}" data-name="{{item.fenzname}}" style="background: linear-gradient(to right, {{Background[index%7]}}  , {{Background1[index%7]}});" wx:for="{{arr}}" wx:key="key">
        <view class="WZwrap-forname">{{item.fenzname}}</view>
        <view class="WZwrap-forname1">
            <view class="WZwrap-forname1textbox">{{item.count?item.count:'0'}}</view>
        </view>
    </view>
    <view class="xx" style="width: 100%;text-align: center;color: #999;line-height: 200rpx;" wx:if="{{arr.length==0}}">暂无分组</view>
</view>
<view class="WZwrap-headline">
  未分组
</view>
<view class="WZ-ungroupedfor" wx:for="{{userlist}}" wx:key="key">
    <view bindtap="shu" class="WZ-ungroupedforimg" data-istg="{{item.istg}}" data-q_id="{{item.q_id}}">
        <image class="WZ-ungroupedforimg1" src="{{item.u_thumb?item.u_thumb:'https://6a69-jin5201-a4503e-1257013711.tcb.qcloud.la/head.png?sign=d02fc6a4cfb712e57b016ddbefcc9c32&t=1578555227'}}"></image>
    </view>
    <view bindtap="shu" class="WZ-ungroupedfortext" data-istg="{{item.istg}}" data-q_id="{{item.q_id}}">
        <view class="remarkName">
            <view>{{item.names}}</view>
        </view>
        <view class="remarkName1" wx:if="{{item.beizhu==null?false:true}}">备注: {{item.beizhu}}</view>
    </view>
    <view bindtap="Wmovement" class="WZ-ungroupedfortext-img" data-beizhu="{{item.beizhu}}" data-q_id="{{item.q_id}}" data-u_id="{{item.u_id}}">
        <image class="WZ-ungroupedfortext-img1" src="https://6a69-jin5201-a4503e-1257013711.tcb.qcloud.la/wzdian.png?sign=6c2d494120ae811742adc20b58d9646f&t=1577154238"></image>
    </view>
</view>
<view class="xx" style="width: 100%;text-align: center;color: #999;line-height: 300rpx;" wx:if="{{userlist.length==0}}">暂无患者</view>
<view class="wz-NewBox" wx:if="{{hiddenmodalput}}">
    <view class="wz-NewBox1">
        <view class="wz-NewBoxheadline">新建组</view>
        <view class="wz-NewBoxseetBox">
            <input autoFocus bindinput="Winput" class="wz-NewBoxseet" maxlength="14" placeholder="新建组名" placeholderStyle="  text-align: center;color: #ccc;" type="text"></input>
        </view>
        <view class="wz-Newfoot">
            <view bindtap="cancel" class="wz-Newfootcancel">取消</view>
            <view bindtap="confirm" class="wz-Newfootcancel1">确定</view>
        </view>
    </view>
</view>
<view class="wz-NewBox" wx:if="{{hiddenmodalput1}}">
    <view class="wz-NewBox1">
        <view class="wz-NewBoxheadline">重命名分组</view>
        <view class="wz-NewBoxseetBox">
            <input autoFocus bindinput="Winputa" class="wz-NewBoxseet" maxlength="14" placeholder="{{creatername}}" placeholderStyle="  text-align: center;color: #ccc;" type="text"></input>
        </view>
        <view class="wz-Newfoot">
            <view bindtap="cancela" class="wz-Newfootcancel">取消</view>
            <view bindtap="confirma" class="wz-Newfootcancel1">确定</view>
        </view>
    </view>
</view>
<navigator class="out_btn" url="/hyb_yl/backstageFollowUp/pages/ChangePatient/ChangePatient?patientType=1&zid={{zid}}">
    <image class="out_image" src="https://6a69-jin5201-a4503e-1257013711.tcb.qcloud.la/out.png?sign=694983653a7fe6fbc70112be4dbefb6c&t=1571646611"></image>
    <text class="font_cofff out_text">发医说</text>
</navigator>
