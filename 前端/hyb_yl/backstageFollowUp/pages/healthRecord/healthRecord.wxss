.wrap {
    width: 100%;
    height: 1200rpx;
    background: #fff;
    position: relative;
    overflow: hidden;
}

.wrap_img {
    width: 100%;
    height: 1000rpx;
    position: absolute;
    top: 60rpx;
    left: 0rpx;
}

.box {
    width: 670rpx;
    height: 1000rpx;
    position: absolute;
    top: 60rpx;
    left: 40rpx;
}

.name {
    width: 100%;
    height: 40rpx;
