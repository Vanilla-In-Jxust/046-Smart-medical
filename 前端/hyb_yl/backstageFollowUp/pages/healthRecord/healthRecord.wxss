    font-size: 34rpx;
    line-height: 40rpx;
    text-align: center;
}

.name_text {
    width: 100%;
    height: 30rpx;
    line-height: 30rpx;
    color: #333;
    text-align: center;
    font-size: 26rpx;
    margin-top: 40rpx;
}

