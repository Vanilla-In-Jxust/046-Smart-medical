::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

page {
    background: #f2f3f7;
}

.nav_view {
    height: 92rpx;
    width: 100%;
}

.nav {
    white-space: nowrap;
    height: 92rpx;
    background: #3b4ca9;
    width: 100%;
}

.nav_text {
    color: #838ec9;
    display: inline-block;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    font-size: 30rpx;
    padding: 0 20rpx;
    max-width: 200rpx;
    position: relative;
}

.active_view {
    width: 40rpx;
    height: 6rpx;
    background-color: #fff;
    border-radius: 3rpx;
    position: absolute;
    bottom: 10rpx;
    left: 50%;
    transform: translateX(-20rpx);
}

.nav_texta {
    color: #fff;
}

.pointing {
    height: 120rpx;
    background: #fff;
    margin: 40rpx 30rpx;
    box-shadow: 0px 10rpx 20rpx 0px #f0f0f0;
    border-radius: 8rpx;
}

.pointing_img {
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin: 41rpx;
}

.pointing_text {
    height: 120rpx;
    line-height: 120rpx;
    font-size: 36rpx;
    color: #333;
    float: left;
}

.pointing_img1_top {
    display: block;
    width: 30rpx;
    height: 30rpx;
    float: right;
    margin-top: 48rpx;
    margin-right: 20rpx;
}

.pointing_img1 {
    display: block;
    width: 30rpx;
    height: 30rpx;
    float: right;
    margin-top: 34rpx;
    margin-right: 20rpx;
}

.date_for {
    width: 100%;
    height: 87rpx;
    background: #fff;
}

.border_top {
    border-top: 1rpx #f0f0f0 solid;
}

.date_for_text {
    font-size: 24rpx;
    color: #333;
    line-height: 87rpx;
    float: left;
    margin-left: 30rpx;
}

.class {
    background-color: linear-gradient(to right,#69f,#3b4ac9);
}

.mg_bottom {
    max-height: 80vh;
}

.position {
    background-color: #f2f3f7;
}

.none_text {
    font-size: 36rpx;
    color: #999;
    display: block;
    height: 500rpx;
    text-align: center;
    line-height: 500rpx;
}

.exa_add_images {
    height: 140rpx;
    width: 140rpx;
    position: fixed;
    z-index: 1000;
    bottom: 0;
    right: 0;
}

.list {
    margin: 0 30rpx;
    background-color: #ffffff;
    box-shadow: 0px 10rpx 20rpx 0px #f0f0f0;
    border-radius: 8rpx;
}

.listmg_top {
    margin-top: 20rpx;
}

.list_title {
    height: 120rpx;
    box-shadow: 0px 10rpx 40rpx 0px #f0f0f0;
