    align-items: center;
    padding: 0 40rpx;
}

.title_left {
    font-size: 48rpx;
}

.tap_img_view {
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #f0f0f0;
}

.tap_view {
    width: 90rpx;
    border-radius: 4rpx;
    font-size: 26rpx;
    text-align: center;
    height: 60rpx;
    line-height: 60rpx;
    color: #999;
}

.tap_view_active {
    background-color: #ffffff;
    border: 4rpx solid #f0f0f0;
    box-sizing: border-box;
    line-height: 56rpx;
    color: #333;
}

.tap_img_left {
    height: 48rpx;
