.W_li {
    width: 100%;
    height: 138rpx;
    background: #fff;
    border-bottom: 1rpx solid #ccc;
    display: flex;
    align-items: center;
}

.W_li_img_box {
    width: 140rpx;
    height: 138rpx;
}

.W_li_img {
    width: 100rpx;
    height: 100rpx;
    display: block;
    margin: 20rpx auto;
    border-radius: 100rpx;
    background: #999;
}

.W_li_text {
    flex: 1;
    height: 138rpx;
    float: left;
    background: #fff;
}

.W_li_text_name {
    height: 40rpx;
    margin-top: 50rpx;
}

.W_li_text_namea {
    float: left;
    color: #333;
    line-height: 40rpx;
    font-size: 30rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.W_li_text_namea_img {
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin-left: 20rpx;
}

.W_li_text_nameDate {
    color: #999;
    font-size: 30rpx;
    height: 40rpx;
    line-height: 40rpx;
    float: left;
    margin-left: 20rpx;
}

.W_li_textA {
    font-size: 24rpx;
    color: #999;
    width: 100%;
    margin-top: 16rpx;
}

.movement {
    width: 120rpx;
    height: 138rpx;
    float: right;
}

.wz-movement {
    width: 40rpx;
    height: 40rpx;
    margin-top: 50rpx;
    margin-left: 10rpx;
}

.setting {
    height: 138rpx;
    float: right;
}

.setting_img {
    width: 50rpx;
    height: 55rpx;
    margin-top: 56rpx;
}

.ling {
    width: 55rpx;
    height: 59rpx;
    float: right;
    margin-top: 30rpx;
    margin-right: 30rpx;
    position: relative;
}

.ling_img {
    width: 55rpx;
    height: 59rpx;
}

.ling_img1 {
    width: 20rpx;
    height: 20rpx;
    position: absolute;
    top: -10rxp;
    right: 0rpx;
}

.wz-wrap {
    width: 100%;
    height: 120rpx;
    background: #fff;
}

.W_seek_input {
    width: 550rpx;
    height: 80rpx;
    background: #f5f5f7;
    border-radius: 10rpx;
    float: left;
    margin-left: 30rpx;
    margin-top: 20rpx;
    font-size: 30rpx;
    position: relative;
}

.W_seek_input1 {
    width: 440rpx;
    height: 80rpx;
    background: #f5f5f7;
    border-radius: 10rpx;
    float: left;
    margin-left: 80rpx;
    font-size: 32rpx;
}

.W_seek_input_img {
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    top: 20rpx;
    left: 30rpx;
}

.W_grouping {
    width: 100%;
    background: #fff;
    margin-top: 20rpx;
    float: left;
    padding-bottom: 60rpx;
}

.seet {
    width: 140rpx;
    height: 80rpx;
    background: linear-gradient(to right,#69f,#3b4ac9);
    float: left;
    font-size: 32rpx;
    line-height: 80rpx;
    text-align: center;
    color: #fff;
    margin-left: 1rpx;
    margin-top: 20rpx;
    border-radius: 8rpx;
}

.remarkName {
    font-size: 24rpx;
    color: #999;
    margin-top: 4rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.distance {
    width: 220rpx;
    display: flex;
    align-items: center;
    padding-right: 20rpx;
    box-sizing: border-box;
}

.distance image {
    width: 30rpx;
    height: 30rpx;
}

.distance text {
    font-size: 24rpx;
    padding-left: 10rpx;
    box-sizing: border-box;
}