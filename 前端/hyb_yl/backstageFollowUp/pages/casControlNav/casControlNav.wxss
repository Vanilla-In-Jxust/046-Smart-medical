.sectionalization {
    width: 100%;
    height: 108rpx;
    line-height: 108rpx;
    text-align: center;
    color: #fff;
    background: #92b1ff;
    font-size: 36rpx;
    float: left;
}

.sectionalization-name {
    width: 80%;
    height: 108rpx;
    text-align: center;
    float: left;
    margin-left: 40rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.W_li {
    width: 100%;
    height: 160rpx;
    background: #fff;
    border-bottom: 1rpx solid #ccc;
    display: flex;
    align-items: center;
}

.W_li_img_box {
    width: 140rpx;
    height: 160rpx;
}

.W_li_img {
    width: 100rpx;
    height: 100rpx;
    display: block;
    margin: 30rpx auto;
    border-radius: 100rpx;
}

.W_li_text {
    flex: 1;
    height: 160rpx;
}

.W_li_text_name {
    height: 40rpx;
    margin-top: 60rpx;
}

.W_li_text_namea {
    float: left;
    color: #333;
    line-height: 40rpx;
    font-size: 30rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.W_li_text_namea_img {
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin-left: 20rpx;
}

.W_li_text_nameDate {
    color: #999;
    font-size: 32rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin-left: 20rpx;
}

.W_li_textA {
    font-size: 30rpx;
    color: #999;
    width: 100%;
    margin-top: 20rpx;
}

.movement {
    width: 120rpx;
    height: 160rpx;
    line-height: 160rpx;
    font-size: 30rpx;
    float: left;
    color: #3b4ca9;
}

.setting {
    height: 160rpx;
    float: right;
}

.setting_img {
    width: 50rpx;
    height: 55rpx;
    margin-top: 56rpx;
    float: right;
}

.sectionalization_img {
    width: 60rpx;
    height: 60rpx;
    float: right;
    margin-top: 24rpx;
    margin-right: 30rpx;
}

.sectionalization_img1 {
    width: 60rpx;
    height: 60rpx;
    float: right;
    margin-top: 50rpx;
    margin-right: 30rpx;
}

.cancel_view {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,.7);
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 10000;
}

.cancel_in {
    background-color: #fff;
    width: 650rpx;
    border-radius: 8rpx;
    min-height: 104rpx;
}

.cancel_in_title {
    border-bottom: 1rpx solid #f0f0f0;
    height: 104rpx;
    line-height: 104rpx;
    text-align: center;
}

.cancel_content {
    padding: 30rpx 30rpx 30rpx 0rpx;
}

.cancel_content_in {
    display: flex;
    flex-wrap: wrap;
    font-size: 26rpx;
    color: #333;
}

.cancel_content_in_top {
    padding-left: 30rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: bottom;
    margin-top: 10rpx;
}

.cancel_text {
    padding: 0 28rpx;
    height: 80rpx;
    background-color: #f6f6f6;
    border-radius: 4rpx;
    line-height: 80rpx;
    text-align: center;
    margin: 30rpx 0 0 30rpx;
}

.cancel_bottom {
    height: 120rpx;
    border-top: 1rpx solid #f0f0f0;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 30rpx;
}

.cancel_bottom_left {
    width: 200rpx;
    height: 80rpx;
    background-color: #f0f0f0;
    border-radius: 4rpx;
    font-size: 32rpx;
    color: #999;
    line-height: 80rpx;
    text-align: center;
}

.cancel_bottom_right {
    width: 370rpx;
    height: 80rpx;
    background-image: linear-gradient(to right,#6699ff,#3b4ca9);
    border-radius: 4rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 32rpx;
    color: #fff;
}

.cancel_active {
    background-color: #e5e9ff;
    color: #3b4ca9;
}

.remarkName {
    font-size: 24rpx;
    margin-top: 4rpx;
    color: #999;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.distance {
    width: 220rpx;
    display: flex;
    align-items: center;
    padding-right: 20rpx;
    box-sizing: border-box;
}

.distance image {
    width: 30rpx;
    height: 30rpx;
}

.distance text {
    font-size: 24rpx;
    padding-left: 10rpx;
    box-sizing: border-box;
}