.seet {
    width: 100%;
    height: 106rpx;
    background: #f5f5f7;
}

.box {
    width: 690rpx;
    height: 64rpx;
    border: 1rpx solid #ccc;
    border-radius: 10rpx;
    margin: 20rpx auto;
    background: #fafafa;
}

.img {
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin-left: 20rpx;
    margin-top: 10rpx;
}

.input {
    width: 500rpx;
    height: 64rpx;
    font-size: 30rpx;
    float: left;
    margin-left: 20rpx;
}

.for_list {
    width: 100%;
    height: 240rpx;
    background: #fff;
    border-bottom: 1rpx #ccc solid;
}

.for_list_auto {
    width: 94%;
    height: 240rpx;
    margin: auto;
}

.for_list_auto_box {
    width: 100%;
    height: 80rpx;
}

.for_list_auto_box_img {
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin-top: 20rpx;
}

.for_list_auto_box_text {
    color: #333;
    font-size: 32rpx;
    float: left;
    line-height: 80rpx;
    margin-left: 20rpx;
}

.for_list_auto_box_text1 {
    color: #ccc;
    font-size: 22rpx;
    float: left;
    line-height: 80rpx;
    margin-left: 20rpx;
}

.for_list_text {
    width: 640rpx;
    height: 156rpx;
    float: left;
    font-size: 26rpx;
    line-height: 38rpx;
    color: #999;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 4;
    overflow: hidden;
}

.for_list_text_img {
    width: 40rpx;
    height: 40rpx;
    float: right;
}