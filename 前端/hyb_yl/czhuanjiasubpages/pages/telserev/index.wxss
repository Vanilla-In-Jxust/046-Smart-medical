page {
    background: white;
}

.all {
    padding: 0px 24rpx;
}

.all .question {
    color: #333;
    font-size: 40rpx;
    line-height: 100rpx;
    margin: 10rpx 0px;
}

.all .title {
    width: 100%;
    display: flex;
    margin-top: 30rpx;
    margin-bottom: 140rpx;
}

.all .title .biao {
    display: block;
    width: 8rpx;
    height: 32rpx;
    margin: 0px 12rpx;
    margin-top: 8rpx;
    background: #46a0f0;
}

.all .title text {
    color: #333;
    font-size: 32rpx;
}

.all .btn {
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: space-between;
}

.all .btn button {
    width: 46%;
    height: 100rpx;
    margin: 0px;
    box-sizing: border-box;
    display: flex;
    justify-content: space-around;
    align-items: center;
    white-space: nowrap;
}

.all .btn button:nth-of-type(1) {
    border: solid 1px #46a0f0;
    color: #46a0f0;
}

.all .btn button:nth-of-type(2) {
    border: solid 1px #46a0f0;
    background: #46a0f0;
    color: white;
}