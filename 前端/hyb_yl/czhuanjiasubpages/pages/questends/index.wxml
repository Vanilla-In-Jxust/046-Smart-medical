<view class="all_two" wx:if="{{datakey==2}}">
    <view class="at_top">
    以下是患者病例
  </view>
    <view class="at_content">
        <view class="at_cont_sign" wx:if="{{sicktel}}">
            <text class="title">联系电话</text>
            <text class="anser">{{sicktel}}</text>
        </view>
        <view class="at_cont_sign" wx:for="{{datalist}}" wx:key="{{index}}">
            <text class="title">{{item.question}}</text>
            <text class="anser">{{item.ansers?item.ansers:item.anser}}</text>
            <text class="contarr" wx:for="{{item.contarr}}" wx:for-item="citem" wx:key="{{index}}">{{citem.value}}</text>
            <block wx:for="{{item.one_items?item.one_items:item.ansers.items}}" wx:for-item="ditem" wx:key="{{index}}">
                <text decode class="one_items" wx:if="{{ditem.value1}}">{{ditem.value1}} &nbsp; {{ditem.value2}}</text>
                <text class="one_items">{{ditem.value}}</text>
            </block>
            <image mode="aspectFit" src="{{eitem}}" wx:for="{{item.img}}" wx:for-item="eitem" wx:key="{{index}}"></image>
        </view>
    </view>
    <view class="at_btn">
        <button bindtap="usecase">
      确认接诊
    </button>
    </view>
</view>
