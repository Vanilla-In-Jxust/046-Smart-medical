page {
    position: relative;
    min-height: 100vh;
    box-sizing: border-box;
    padding-bottom: 180rpx;
}

view {
    box-sizing: border-box;
}

.all_one {
    width: 100%;
}

.one {
    width: 100%;
    height: auto;
    background: white;
}

.sign {
    width: 100%;
    padding: 30rpx 24rpx;
    border-bottom: solid 1px rgb(209,209,209);
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 28rpx;
    position: relative;
}

.one .sign .sign_l {
    color: #999;
}

.one .sign .sign_r {
    color: #333;
}

.two {
    margin-top: 30rpx;
    background: white;
}

.two .signs_l {
    color: #333;
}

.two .signs_r {
    color: #ed9a56;
}

.three {
    font-size: 26rpx;
    color: #4fb3fe;
    margin-left: 24rpx;
    display: flex;
    align-items: center;
    margin-top: 30rpx;
}

.arrowc {
    width: 14rpx;
    height: 14rpx;
    border-top: solid 1px #4fb3fe;
    border-right: solid 1px #4fb3fe;
    transform: rotate(45deg);
}

.ident {
    width: 100%;
}

.ident text {
    float: right;
    display: block;
    margin-right: 24rpx;
    margin-top: 20rpx;
    font-size: 28rpx;
    color: #999;
}

.arrow {
    width: 20rpx;
    height: 20rpx;
    border-top: solid 1px #999;
    border-right: solid 1px #999;
    transform: rotate(45deg) translateY(-50%);
    position: absolute;
    top: 50%;
    right: 35rpx;
}

.state .posfix {
    width: 100%;
    height: 100rpx;
    position: fixed;
    bottom: 0px;
    left: 0px;
    background: white;
    display: flex;
}

.state .posfix view {
    display: flex;
    align-items: center;
}

.state .posfix .posf_l {
    width: 70%;
    height: 100%;
}

.state .posfix .posf_l text:nth-of-type(1) {
    margin: 0px 20rpx;
}

.state .posfix .posf_l text:nth-of-type(2) {
    color: #ed9a56;
}

.state .posfix .posf_r {
    width: 30%;
    height: 100%;
    justify-content: space-around;
    background: #4fb3fe;
}

.posf_r button {
    color: white;
    border: none;
    width: 100%;
    height: 100%;
    background: #4fb3fe;
}

.all_two {
    width: 100%;
}

.all_two .at_top {
    width: 100%;
    background: white;
    padding: 30rpx;
    font-size: 32rpx;
    font-weight: bold;
    line-height: 50rpx;
}

.all_two .at_content {
    width: 94%;
    height: auto;
    margin: auto;
    background: white;
    margin-top: 20rpx;
    padding: 30rpx 24rpx;
    border-radius: 10rpx;
}

.all_two .at_content .at_cont_sign {
    width: 100%;
    font-size: 30rpx;
    padding: 10rpx 0px;
    border-bottom: solid 1px rgb(223,221,221);
}

.all_two .at_content .at_cont_sign text {
    display: block;
    line-height: 50rpx;
}

.all_two .at_content .at_cont_sign .title {
    color: #666;
}

.all_two .at_content .at_cont_sign .anser {
    color: #333;
}

.all_two .at_content .at_cont_sign image {
    width: 100rpx;
    height: 100rpx;
}

.all_two .at_btn {
    width: 100%;
    height: 120rpx;
    background: white;
    display: flex;
    align-items: center;
    justify-content: space-around;
    position: fixed;
    left: 0px;
    bottom: 0px;
}

.all_two .at_btn button {
    width: 45%;
    height: 90rpx;
    display: flex;
    align-items: center;
    justify-content: space-around;
    font-size: 36rpx;
}

.all_two .at_btn button:nth-of-type(1) {
    border: solid 1px #47a1ef;
    color: #47a1ef;
    background: white;
}

.all_two .at_btn button:nth-of-type(2) {
    border: solid 1px #47a1ef;
    color: white;
    background: #47a1ef;
}