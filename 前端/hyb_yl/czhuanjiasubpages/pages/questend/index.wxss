page {
    position: relative;
    min-height: 100vh;
    box-sizing: border-box;
}

view {
    box-sizing: border-box;
}

.all {
    width: 100%;
    padding: 0px 24rpx;
}

.tel {
    width: 100%;
    height: auto;
}

.tel .tel_text {
    width: 100%;
    padding: 35rpx 0px;
    color: #333;
    font-size: 34rpx;
}

.tel .tel_input {
    width: 100%;
    height: 100rpx;
}

.tel .tel_input input {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    border: solid 1px #dcdcdc;
    text-align: center;
    background: #f5f5f5;
}

.tel .tel_btn {
    width: 100%;
    height: 60rpx;
    margin-top: 50rpx;
    display: flex;
}

.tel .tel_btn button {
    margin: auto;
    padding: 0px;
    background: #46a0f0;
    color: white;
    width: 300rpx;
    height: 80rpx;
    display: flex;
    align-items: center;
    justify-content: space-around;
}