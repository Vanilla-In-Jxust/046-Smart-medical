page {
    width: 100%;
    background: #FFFFFF;
    box-sizing: border-box;
    padding-bottom: 190rpx;
}

view {
    box-sizing: border-box;
}

.headbox {
    padding: 25rpx;
    padding-bottom: 0;
    height: 380rpx;
    background: #004cbe;
    border-radius: 20rpx;
    background-image: url("https://6a69-jin5201-a4503e-1257013711.tcb.qcloud.la/yuanchengguahao.png?sign=b6c755e08a53e57593b6a33285c9fdde&t=1584171359");
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.bigTitle {
    font-size: 45rpx;
    font-weight: bold;
    padding-left: 35rpx;
    padding-top: 35rpx;
    color: #FFFFFF;
    margin-bottom: 30rpx;
}

.smallTitlte {
    color: #daddff;
    font-size: 22rpx;
    margin-left: 32rpx;
}

.smallfont {
    margin-bottom: 15rpx;
}

.smallfont text {
    margin-right: 15rpx;
}

.commonbox {
    padding: 25rpx;
    padding-top: 30rpx;
}

.bottomborder {
    border-bottom: 25rpx solid #F6F6F6;
}

.commTitlebox {
    display: flex;
    justify-content: space-between;
    margin-bottom: 30rpx;
}

.title_left {
    padding-left: 15rpx;
    font-weight: bold;
    color: #333;
    font-size: 32rpx;
    position: relative;
}

.title_left::after {
    content: "";
    width: 6rpx;
    height: 80%;
    background: #0088de;
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
}

.lookeval {
    color: #0088de;
    font-size: 28rpx;
}

.evalCont {
    font-size: 30rpx;
    color: #333;
}

.czlist {
    line-height: 40rpx;
}

.btn {
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #fff;
    padding: 25rpx;
    padding-bottom: 50rpx;
}

.now {
    width: 100%;
    background: #0088DE;
    font-size: 32rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 90rpx;
    color: #fff;
    border-radius: 50rpx;
}

.liuchengbox {
    background: #FAFAFA;
    padding: 0 25rpx;
    border-radius: 10rpx;
}

.stepliucheng {
    padding: 10rpx 0;
}

.liuchengitem {
    width: 100%;
    display: flex;
    align-items: center;
    padding: 20rpx 0;
}

.liuchengitemnum {
    width: 38rpx;
    height: 38rpx;
    margin-right: 15rpx;
    border-radius: 50%;
    text-align: center;
    line-height: 37rpx;
    font-size: 26rpx;
    color: #E5E5E5;
    background: #989898;
    position: relative;
}

view.lcline::after {
    content: "";
    width: 1rpx;
    height: 80rpx;
    border-left: 1rpx solid #BEBEBE;
    position: absolute;
    top: 100%;
    left: 50%;
}

.liuchengitemtext {
    font-size: 30rpx;
    color: #5E5E5E;
}

.liuchengitemtext text {
    color: #E92D2D;
}

.xieyishuoming {
    padding: 20rpx 0;
    border-top: 1rpx solid #F6F6F6;
    font-size: 26rpx;
    color: #A4A4A4;
}

.xieyishuoming view {
    padding: 10rpx;
    position: relative;
    padding-left: 25rpx;
}

.xieyishuoming view::after {
    content: "";
    width: 10rpx;
    height: 10rpx;
    position: absolute;
    border-radius: 50%;
    background: #D5D5D5;
    top: 25rpx;
    left: 0;
}

.radios {
    font-size: 32rpx;
    color: #7F7F7F;
    text-align: left;
    display: inline-flex;
    align-items: center;
    padding: 0 25rpx;
}

.radios radio {
    transform: scale(0.8);
    margin-left: -25rpx;
}

.radios>text {
    font-size: 28rpx;
    margin-left: -15rpx;
}

.zhe {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.4);
    display: flex;
    justify-content: center;
    align-items: center;
}

.zhe .cont {
    width: 80%;
    background: #fff;
    border-radius: 10rpx;
    padding: 60rpx 40rpx;
}

.rule_box {
    height: 418rpx;
    overflow: hidden;
    overflow-y: auto;
}

.zhe .title {
    font-size: 36rpx;
    text-align: center;
    font-weight: bold;
    color: #666;
    margin-bottom: 30rpx;
}

.rule_btn {
    width: 100%;
    height: 88rpx;
    font-size: 32rpx;
    color: #fff;
    text-align: center;
    line-height: 88rpx;
    border-radius: 20rpx;
    margin: 30rpx auto;
    background: #004cbe;
}

.W_name {
    width: 100%;
    height: 100rpx;
    border-bottom: 1rpx solid #f0f0f0;
    background: #fff;
    padding: 0;
    display: flex;
    justify-content: space-between;
}

.W_imgName1 {
    height: 100rpx;
    float: left;
    line-height: 100rpx;
    text-indent: 30rpx;
    font-size: 30rpx;
    color: #333;
    white-space: nowrap;
}