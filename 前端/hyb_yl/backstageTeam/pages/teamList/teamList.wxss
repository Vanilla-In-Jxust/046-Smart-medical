page {
    background-color: #f5f5f7;
}

.list_head {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100;
    height: 100rpx;
    width: 100%;
    background-color: #fff;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.list_head view {
    line-height: 95rpx;
    margin-bottom: 5rpx;
    box-sizing: border-box;
    border-bottom: 4rpx solid #fff;
}

.list_head .active {
    border-bottom: 4rpx solid #3b4ca9;
    color: #3b4ca9;
}

.classify_text_red {
    display: block;
    width: 20rpx;
    height: 20rpx;
    background: red;
    position: fixed;
    top: 15rpx;
    right: 110rpx;
    border-radius: 20rpx;
    z-index: 999;
}

.list_view {
    padding: 30rpx;
    background-color: #fff;
    display: flex;
}

.list_icon {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    margin-right: 30rpx;
    float: left;
}

.list_content {
    float: left;
}

.title_top {
    margin-bottom: 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.content_title {
    width: 400rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.content_bottom {
    display: flex;
}

.content_bottom_left {
    margin-right: 58rpx;
}

.content_bottom_left_right {
    margin-left: 16rpx;
}

.add_view {
    height: 104rpx;
    background-color: #fff;
    display: flex;
    align-items: center;
    padding-left: 60rpx;
    margin-bottom: 20rpx;
    justify-content: space-between;
}

.more_images {
    width: 40rpx;
    height: 40rpx;
    display: block;
    margin-right: 30rpx;
}

.add_img {
    width: 40rpx;
    height: 40rpx;
    margin-right: 60rpx;
}

.border_top {
    border-top: 1rpx solid #f0f0f0;
}

.scroll_view {
    max-height: 100vh;
}

.none_team {
    height: 500rpx;
    line-height: 500rpx;
    text-align: center;
}

.join {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 32rpx;
    text-align: center;
    background: #fff;
    margin-top: 30rpx;
    color: #3b4ca9;
}

.wz {
    background: #f0f0f0;
    width: 140rpx;
    height: 60rpx;
    border-radius: 60rpx;
    line-height: 60rpx;
    text-align: center;
    float: left;
}

.wznavigator {
    width: 80%;
    float: left;
}

.wz_Isnotset {
    width: 100%;
    color: red;
    font-size: 24rpx;
    float: left;
    margin-top: 10rpx;
}

.wz_hint {
    width: 160rpx;
    float: right;
}

.wz_tablist {
    width: 100%;
    height: 100rpx;
    background-color: #f5f5f5;
}

.wz_tablist1 {
    width: 50%;
    height: 100rpx;
    float: left;
    background: #fff;
    line-height: 100rpx;
    text-align: center;
    font-size: 28rpx;
    color: #999;
    background-color: #f5f5f5;
    position: relative;
}

.active1 {
    width: 50%;
    height: 100rpx;
    float: left;
    background: #fff;
    line-height: 100rpx;
    text-align: center;
    font-size: 28rpx;
    color: #333;
    background: #fff;
    position: relative;
}

.wz_shanjiao {
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    width: 30rpx;
    height: 100rpx;
}

.wz_shanjiao1 {
    position: absolute;
    left: 0rpx;
    top: 0rpx;
    width: 30rpx;
    height: 100rpx;
}