page {
    background-color: #f5f5f7;
}

.icon {
    margin-top: 20rpx;
    height: 140rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background-color: #fff;
    margin: 0 50rpx 0 50rpx;
    border-bottom: 1rpx solid #f0f0f0;
}

.title {
    width: 200rpx;
}

.data-item {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
    height: 88rpx;
    border-bottom: 1rpx solid #EAEAEA;
    font-size: 28rpx;
    color: #333;
}

.pho-icon {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
}

.icon_img {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}

.icon_more {
    width: 40rpx;
    height: 40rpx;
    display: block;
    transform: translate(10rpx,4rpx);
}

.icon_right {
    display: flex;
    align-items: center;
}

.team_name {
    display: flex;
    height: 104rpx;
    align-items: center;
    background-color: #fff;
    margin: 0 50rpx;
}

.team_name input {
    margin-left: 60rpx;
    flex: 1;
    text-align: right;
}

.infro_view {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 96rpx;
    background-color: #fff;
    margin: 0 50rpx 0 50rpx;
    border-top: 1rpx solid #f0f0f0;
}

.infro_view_intro {
    height: 110rpx;
}

.city_right {
    display: flex;
    align-items: center;
}

.foot_view {
    position: fixed;
    z-index: 100;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 104rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.foot_btn {
    width: 690rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
}

.intro {
    width: 300rpx;
    text-align: right;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.save_view {
    height: 104rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 50rpx 0;
}

.drop_btn {
    width: 690rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    background-color: #f0f0f0;
    border-radius: 40rpx;
}

.rgb {
    background-color: rgba(0,0,0,0.5);
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1000;
}

.selector-area {
    height: 100%;
    width: 600rpx;
    background-color: #fff;
    position: fixed;
    top: 0;
    right: 0;
    z-index: 100000;
    display: flex;
    flex-direction: column;
    transform: translateX(600rpx);
}

.area-selector {
    display: flex;
    flex: 1;
}

.handle-bar {
    height: 88rpx;
    display: flex;
    align-items: center;
}

.handle-bar image {
    width: 40rpx;
    height: 40rpx;
    margin-left: 24rpx;
}

.handle-bar .hd_box {
    flex: 1;
    text-align: center;
}

.selector {
    width: 300rpx;
    height: 92.5vh;
}

.picker {
    width: 300rpx;
    height: 88rpx;
    font-size: 28rpx;
    color: #333;
    text-align: center;
    line-height: 88rpx;
}

.province {
    background: #fff;
}

.city {
    background: #f7f7f7;
}

.province-active {
    background-color: #f7f7f7;
    color: #3b4ca9;
}

.city-active {
    background-color: #f0f0f0;
    color: #3b4ca9;
}

textarea {
    padding: 0 50rpx 50rpx;
    width: 100%;
    box-sizing: border-box;
    font-size: 32rpx;
    color: #333;
}

.team-box {
    padding: 24rpx;
}

.team-create {
    width: 654rpx;
    padding: 0 24rpx;
    background: #fff;
    border-radius: 30rpx;
    margin-bottom: 23rpx;
}

.team-item {
    display: flex;
    padding: 30rpx 0;
    border-bottom: 1rpx solid #EAEAEA;
}

.team-tit {
    width: 240rpx;
    font-size: 28rpx;
    color: #333;
}

.plafont {
    font-size: 28rpx;
    color: #999;
}

.team-item:last-child {
    border-bottom: 0;
}

.radio {
    margin-right: 30rpx;
    font-size: 26rpx;
    color: #999;
}

.picker {
    width: 380rpx;
    height: 40rpx;
    text-align: left;
}

.picker-tt {
    height: 40rpx;
    line-height: 45rpx;
    font-size: 26rpx;
    color: #999;
}

.next {
    width: 50rpx;
}

.team-tip-t {
    font-size: 26rpx;
    color: #999;
}

.be-list {
    display: flex;
    flex-wrap: wrap;
    width: 100%;
    padding-top: 25rpx;
}

.be-list-item {
    padding: 8rpx 20rpx;
    border: 1rpx solid #D2D2D2;
    color: #D2D2D2;
    margin: 0 24rpx 24rpx 0;
    border-radius: 23rpx;
    font-size: 26rpx;
}

.be-list-item-act {
    border-color: #0087E0;
    color: #0087E0;
}

.bref {
    padding: 30rpx 0;
    height: 150rpx;
}

.sure-create {
    width: 500rpx;
    height: 88rpx;
    margin: 30rpx 125rpx;
    font-size: 32rpx;
    color: #fff;
    border-radius: 30rpx;
    background: #0087E0;
}