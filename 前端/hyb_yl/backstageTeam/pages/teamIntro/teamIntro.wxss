page {
    background: #fff;
}

.team_head_view {
    padding: 0 30rpx 40rpx 30rpx;
    margin-bottom: 20rpx;
    background-color: #fff;
}

.team_head {
    display: flex;
    align-items: center;
    padding: 36rpx 0;
}

.city {
    display: inline-block;
    margin: 0 15rpx 0 15rpx;
}

.team_head_left {
    flex: 1;
}

.eait_img_view {
    display: inline-block;
}

.edit_img {
    height: 36rpx;
    width: 30rpx;
    margin: 0 20rpx;
    transform: translateY(6rpx);
}

.edit_img_small {
    height: 30rpx;
    width: 24rpx;
    margin: 0 20rpx;
    transform: translateY(6rpx);
}

.team_img {
    width: 50rpx;
    height: 50rpx;
    margin-left: 80rpx;
}

.list_96 {
    height: 96rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background-color: #FFF;
    padding: 0 20rpx 0 30rpx;
}

.border_bottom {
    border-bottom: 1rpx solid #f0f0f0;
}

.more_img {
    height: 40rpx;
    width: 40rpx;
    margin-left: 5rpx;
}

.list_left {
    display: flex;
    align-items: center;
}

.server_view {
    height: 80rpx;
    background-color: #fff;
    padding: 0 30rpx;
    line-height: 80rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.server_content {
    padding: 0 30rpx 10rpx 30rpx;
    background-color: #fff;
}

.server_left {
    display: flex;
    align-items: center;
    height: 80rpx;
    justify-content: space-between;
}

.server_left_left {
    display: flex;
    align-items: center;
}

.server_list {
    display: flex;
    height: 80rpx;
    align-items: center;
    justify-content: space-between;
    padding-left: 56rpx;
}

.server_img {
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
}

.new_price {
    color: #f42c4d;
}

.old_price {
    margin-left: 15rpx;
    color: #999;
    text-decoration: line-through;
}

.set_team_img {
    margin-top: 20rpx;
    height: 140rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: #fff;
    padding: 0 20rpx 0 30rpx;
    border-bottom: 1rpx solid #f0f0f0;
}

.team_headimg {
    height: 100rpx;
    width: 100rpx;
    border-radius: 50%;
}

.add_view {
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: #fff;
    padding: 0 30rpx;
    height: 104rpx;
    margin: 20rpx 0 0 0;
}

.add_left {
    display: flex;
    align-items: center;
    flex: 1;
}

.add_person_img {
    height: 40rpx;
    width: 40rpx;
    margin-right: 30rpx;
    margin-left: 30rpx;
}

.add_right_img {
    height: 50rpx;
    width: 50rpx;
}

.hoverClass {
    background-color: #fff;
}

.join {
    width: 100%;
    height: 92rpx;
    position: fixed;
    left: 0rpx;
    bottom: 0rpx;
    color: #fff;
    background: linear-gradient(to right,#6699ff,#3b4ac9);
    font-size: 32rpx;
    line-height: 92rpx;
    text-align: center;
}

.content_view {
    padding: 20rpx 60rpx 0rpx;
}

.content_team_img {
    display: flex;
    align-items: center;
    justify-content: center;
}

.content_left {
    display: flex;
    align-items: center;
}

.set_img {
    width: 50rpx;
    height: 50rpx;
    display: block;
    margin-right: 50rpx;
}

.team_ma_img {
    height: 50rpx;
    width: 50rpx;
    display: block;
}

.team_img_view {
    width: 180rpx;
    height: 180rpx;
    border: solid 2rpx #f0f0f0;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 50%;
    margin: 0 30rpx 0 70rpx;
}

.team_image {
    height: 140rpx;
    width: 140rpx;
    display: block;
    border-radius: 50%;
}

.doyen {
    display: flex;
    align-items: center;
}

.doyen_img {
    height: 90rpx;
    width: 90rpx;
    display: block;
    margin-right: 18rpx;
    border-radius: 50%;
}

.content_teamName {
    text-align: center;
    padding: 20rpx 0 36rpx;
}

.content_team_intro_active {
    max-height: 120rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
}

.content_team_intro {
    margin-bottom: 80rpx;
    position: relative;
}

.down_image_view {
    position: absolute;
    bottom: -50rpx;
    right: -50rpx;
    width: 130rpx;
    height: 100rpx;
    z-index: 1000;
}

.down_image {
    width: 30rpx;
    height: 30rpx;
    transform: translateY(10rpx);
    position: absolute;
    bottom: 40rpx;
    right: 0rpx;
    z-index: 1000;
    margin-bottom: 20rpx;
    margin-left: 50rpx;
    margin-top: 50rpx;
    margin-right: 50rpx;
}

.content_sort {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.content_list {
    width: 300rpx;
    height: 200rpx;
    border-radius: 32rpx 8rpx 32rpx 8rpx;
    padding: 30rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin: 0 4rpx 20rpx;
}

.content_list_notice {
    background-color: #4db5fe;
}

.notice_img {
    height: 50rpx;
    width: 50rpx;
    margin-left: 136rpx;
}

.serve_img {
    margin-left: 0;
}

.serve_view {
    margin-right: 100rpx;
    line-height: 60rpx;
}

.notice_news {
    width: 56rpx;
    height: 56rpx;
    background-color: #ff4c4c;
    border-radius: 50%;
    line-height: 56rpx;
    text-align: center;
    color: #fff;
}

.content_list_notice_top {
    display: flex;
    justify-content: flex-end;
}

.content_list_text {
    font-size: 60rpx;
    color: #fff;
}

.content_list_burse {
    width: 620rpx;
}

.test {
    width: 156rpx;
    padding-left: 150rpx;
    height: 40rpx;
    position: absolute;
    right: 0;
    bottom: 0;
    background: linear-gradient(to right,rgba(255,255,255,0),white 50%,white);
    line-height: 40rpx;
}

.test_text {
    opacity: 0.7;
}