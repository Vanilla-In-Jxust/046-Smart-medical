page {
    background: #f2f3f7;
}

.change_view {
    line-height: 96rpx;
    flex: 1;
    text-align: center;
    border-bottom: 4rpx solid #fff;
}

.active {
    color: #3b4ca9;
    border-bottom: 4rpx solid #3b4ca9;
}

.change_body {
    position: relative;
    height: 100%;
    margin-top: 96rpx;
}

.bg_img {
    display: block;
    width: 100%;
    position: absolute;
    left: 0;
    z-index: -1;
    top: 96rpx;
}

.text_sao {
    padding: 180rpx 0;
}

.text_text {
    height: 48rpx;
    line-height: 48rpx;
    text-align: center;
}

.scan_view {
    margin: 0 auto;
}

.scroll-view {
    max-height: 76vh;
    margin-top: 225rpx;
}

.search_view {
    padding: 0 24rpx;
    height: 96rpx;
}

.search {
    line-height: 96rpx;
    padding: 5rpx;
    flex: 1;
}

.serach_img {
    width: 32rpx;
    height: 32rpx;
    margin-right: 10rpx;
}

.scope_view {
    padding: 0 34rpx;
    border-top: 1rpx solid #f0f0f0;
    border-bottom: 1rpx solid #f0f0f0;
    box-sizing: border-box;
    line-height: 88rpx;
}

.scope {
    width: 33.33%;
    justify-content: center;
}

.choose_name {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.hospital_scope {
    justify-content: flex-start;
}

.hospital_name {
    width: 160rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: right;
}

.city_img {
    height: 20rpx;
    width: 20rpx;
    margin-left: 14rpx;
}

.doctor_view {
    padding: 36rpx 40rpx 0rpx 40rpx;
    position: relative;
}

.doctor_img {
    width: 80rpx;
    height: 80rpx;
    margin-right: 20rpx;
    border-radius: 50%;
    margin-top: 10rpx;
}

.doctor_view_right {
    display: flex;
}

.name_view {
    flex: 1;
    padding-bottom: 20rpx;
    display: flex;
    justify-content: space-between;
}

.name_text {
    margin-right: 20rpx;
}

.img_right {
    display: flex;
    align-items: center;
}

.be_good {
    padding-top: 18rpx;
    display: flex;
    align-items: center;
}

.hospital_view {
    max-width: 320rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    margin-right: 20rpx;
}

.job {
    display: inline-block;
    margin-right: 10rpx;
}

.be_good_cotent {
    margin: 16rpx 0 30rpx 0;
    overflow: hidden;
}

.z_index {
    z-index: 10000;
    position: relative;
}

.noData {
    height: 150rpx;
    line-height: 150rpx;
    text-align: center;
}

.fullbg {
    position: fixed;
    top: 0;
    z-index: 1;
    width: 100%;
    height: 100%;
    background: rgb(1,1,1);
    transition: all 2s;
    opacity: 0;
}

.fullopacity {
    opacity: 0.5;
}

.dis_none {
    display: none;
}

.change_area {
    display: none;
    height: 500rpx;
    position: relative;
    display: -webkit-flex;
    display: flex;
    overflow: hidden;
}

.change_office,.change_hospital {
    display: none;
    height: 500rpx;
    position: relative;
    overflow: hidden;
}

.office_text,.hospital_text {
    height: 500rpx;
    padding-left: 40rpx;
    line-height: 80rpx;
    overflow-y: scroll;
}

.office_text view,.hospital_text view {
    border-bottom: 1rpx solid #f0f0f0;
}

.province {
    overflow-y: scroll;
    float: left;
    width: 50%;
    height: 500rpx;
    line-height: 80rpx;
}

.city {
    overflow-y: scroll;
    width: 50%;
    height: 500rpx;
    line-height: 80rpx;
}

.province_view {
    text-align: center;
    background-color: #fff;
}

.city_view {
    text-align: center;
    background-color: #f7f7f7;
}

.province_active {
    background-color: #f7f7f7;
    color: #3b4ca9;
}

.city_active {
    background-color: #f0f0f0;
    color: #3b4ca9;
}

.hospital_active {
    color: #3b4ca9;
}

@-webkit-keyframes slidown {
    from {
        transform: translateY(-100%);
    }

    to {
        transform: translateY(0%);
    }
}

@keyframes slidown {
    from {
        transform: translateY(-100%);
    }

    to {
        transform: translateY(0%);
    }
}

.slidown {
    display: block;
    animation: slidown 0.5s ease-in both;
}

@-webkit-keyframes slidup {
    from {
        transform: translateY(0%);
    }

    to {
        transform: translateY(-100%);
    }
}

@keyframes slidup {
    from {
        transform: translateY(0%);
    }

    to {
        transform: translateY(-100%);
    }
}

.slidup {
    display: none;
    animation: slidup 0.5s ease-in both;
}

.disappear {
    display: none;
}

.nmHospital {
    flex: 1;
}

.shop_view {
    width: 88rpx;
    height: 36rpx;
    background-color: #ffebee;
    border-radius: 8rpx;
    line-height: 36rpx;
    text-align: center;
    margin-left: 20rpx;
}

.former {
    text-decoration: line-through;
}

.people_number {
    display: inline-block;
    margin-left: 14rpx;
}

.service {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-family: PingFang-SC-Medium;
    margin: 0 40rpx 0 40rpx;
    height: 80rpx;
    border-top: 1rpx solid #f0f0f0;
}

.now_pric {
    margin-right: 20rpx;
}

.service_view_img {
    display: flex;
    align-items: center;
}

.service_img {
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
}

.sign {
    width: 120rpx;
    height: 60rpx;
    border-radius: 30rpx;
    border: 1rpx solid #3b4ca9;
    color: #3b4ca9;
    margin-left: 20rpx;
    line-height: 60rpx;
    text-align: center;
    position: absolute;
    right: 30rpx;
    top: 30rpx;
}

.sign_none {
    border: 1rpx solid #ccc;
    color: #ccc;
    width: 120rpx;
    height: 60rpx;
    border-radius: 30rpx;
    margin-left: 20rpx;
    line-height: 60rpx;
    text-align: center;
}

.sign_number {
    display: flex;
}

.sign_number view {
    flex: 1;
}