page {
    position: relative;
    background-color: #f2f2f2;
    box-sizing: border-box;
    padding-bottom: 30rpx;
}

.tiwen-bg {
    width: 100%;
    height: 50rpx;
}

.nav-scr {
    box-shadow: 1rpx 1rpx 30rpx 0.5rpx #e2e2e2;
}

.ti-btn::after {
    border: 1rpx solid #02b3b9;
}

radio {
    transform: scale(.7,.7);
}

.money_box {
    display: flex;
    align-items: center;
    flex-wrap: wrap;
}

.money {
    width: 30%;
    border: 2rpx solid #999;
    color: #999;
    font-size: 28rpx;
    height: 98rpx;
    line-height: 98rpx;
    border-radius: 10rpx;
    text-align: center;
    margin-right: 10rpx;
    margin-bottom: 20rpx;
    position: relative;
    overflow: hidden;
}

.money:nth-child(3n) {
    margin-right: 0;
}

.jiaobiao {
    position: absolute;
    top: 0;
    left: 0;
}

.monye_active {
    color: #02b3b9;
    border-color: #02b3b9;
}

.money_title {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 24rpx 0;
    font-size: 30rpx;
}

.bgf {
    background: #fff;
    padding-bottom: 20rpx;
}

.btn_text {
    font-size: 26rpx;
    color: #02b3b9;
}

.rule {
    background: rgb(246,247,249);
    color: #999;
    font-size: 24rpx;
    padding: 24rpx;
    border-radius: 10rpx;
}

.rule_title {
    font-size: 30rpx;
    color: #333;
    margin-bottom: 20rpx;
}

.df {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.tixianfangshi {
    background: #fff;
    padding: 0 20rpx 20rpx;
    margin-top: 20rpx;
}

.tixain_title {
    padding: 24rpx 0;
    color: #333;
    font-size: 30rpx;
}

.pdtb24 {
    padding: 24rpx 0;
    color: #333;
    font-size: 26rpx;
}

.money1 {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx;
    font-size: 30rpx;
    color: #333;
    margin-top: 10rpx;
}

.nav {
    background: #fff;
    padding: 20rpx;
    box-sizing: border-box;
}

.swiper_box {
    height: 98rpx;
    background: rgb(255,246,211);
    color: #E4BE65;
}

.swiper_box swiper {
    height: 100%;
}

.swiper_box swiper-item {
    text-align: center;
    display: flex;
    align-items: center;
    justify-content: center;
}

.mt20 {
    margin-top: 20rpx;
}

.c9 {
    color: #999;
}

.fs24 {
    font-size: 24rpx;
}