<view class="bsbb pl18 pr18 mt20 mb50" wx:if="{{list.length!=0}}">
    <view class="bsbb pl28 pr28 bgcf bdr20">
        <view class="bdb2 df jcsb aic bsbb pt28 pb28" wx:for="{{list}}" wx:key="unique">
            <view class="df fdc jcsa">
                <text class="fs30">{{item.title}}</text>
                <text class="fs26 c9">{{item.created}}</text>
            </view>
            <view class="fs30 c9">-{{item.money}}</view>
        </view>
    </view>
</view>
<view class="none" wx:else>
    <image mode="widthFix" src="../images/none.png"></image>
</view>
