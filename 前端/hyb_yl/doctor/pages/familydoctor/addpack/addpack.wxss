page {
    width: 100%;
    height: 100%;
    background: #f8f8f8;
}

.oval {
    padding-left: 27rpx;
    padding-right: 27rpx;
}

.packlist {
    width: 100%;
    display: flex;
    height: 100rpx;
    background: #fff;
    margin-top: 20rpx;
    border-radius: 22rpx;
    font-size: 30rpx;
    color: #666666;
    justify-content: space-between;
}

.packtitle {
    line-height: 100rpx;
    padding-left: 30rpx;
}

.packright {
    margin-right: 27rpx;
    padding-top: 3.5rpx;
    line-height: 100rpx;
}

.cameImg {
    width: 80rpx;
    height: 80rpx;
    line-height: 100rpx;
}

.cameImg image {
    width: 100%;
    height: 100%;
}

.packImg {
    width: 92rpx;
    height: 92rpx;
    border-radius: 20rpx;
}

.packImg image {
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
}

.cont {
    margin-top: 20rpx;
    width: 100%;
    border-radius: 20rpx;
    background: white;
}

.contlist {
    padding-left: 22rpx;
    padding-right: 26rpx;
}

.contlistbox {
    display: flex;
    justify-content: space-between;
    border-bottom: 5rpx dashed #ccc;
    height: 100rpx;
}

.listLeft {
    color: #666666;
    font-size: 28rpx;
    line-height: 100rpx;
    width: 150px;
}

.throng {
    font-size: 26rpx;
    color: #333;
    margin-right: 20rpx;
    text-align: right;
}

.throng input {
    width: 100%;
    height: 100%;
}

.listRight {
    display: flex;
    justify-content: flex-end;
}

.arrowImg {
    line-height: 100rpx;
}

.arrowImg image {
    width: 32rpx;
    height: 24rpx;
}

.period {
    display: flex;
    justify-content: space-between;
}

.dredgebox {
    margin-top: 24rpx;
}

.dre {
    width: 100%;
    height: 100rpx;
    background: white;
    border-radius: 20rpx;
    display: flex;
    justify-content: space-between;
}

.dre_left {
    color: #666666;
    font-size: 28rpx;
    line-height: 100rpx;
    margin-left: 24rpx;
}

.dre_right {
    margin-right: 24rpx;
    line-height: 96rpx;
}

.wx-switch-input {
    width: 90rpx!important;
    height: 50rpx!important;
    background: #5CC9AC!important;
    border: #ccc!important;
}

.wx-switch-input::before {
    width: 90rpx!important;
    height: 50rpx!important;
}

.wx-switch-input::after {
    width: 45rpx!important;
    height: 50rpx!important;
}

.choosebox {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0rpx;
    top: 0rpx;
    background: rgba(0,0,0,0.3);
    z-index: 1;
}

.crowdbox {
    width: 100%;
    height: 680rpx;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
    background: white;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    z-index: 9;
}

.choosetitle {
    text-align: center;
    margin-top: 70rpx;
    color: #333;
    font-size: 32rpx;
}

.crowdcenter {
    margin-top: 30rpx;
}

.crowlist {
    height: 90rpx;
    display: flex;
    justify-content: space-between;
    padding-left: 25rpx;
    padding-right: 50rpx;
    line-height: 90rpx;
}

.crowlist_active {
    background: rgba(92,201,172,0.3);
}

.pitch {
    width: 30rpx;
    height: 30rpx;
}

.show {
    display: block;
}

.hide {
    display: none;
}

.footbox {
    width: 100%;
    position: fixed;
    left: 0rpx;
    bottom: 90rpx;
    padding-left: 25rpx;
    padding-right: 25rpx;
}

.add {
    width: 700rpx;
    background: #5CC9AC;
    height: 96rpx;
    border-radius: 20rpx;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 30rpx;
}

.addImg {
    width: 45rpx;
    height: 45rpx;
    border-radius: 50%;
    margin-right: 19rpx;
}

.addImg image {
    width: 100%;
    height: 100%;
}