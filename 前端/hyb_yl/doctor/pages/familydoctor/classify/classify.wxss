page {
    width: 100%;
    height: 100%;
    background: #f8f8f8;
}

.oval {
    padding-left: 25rpx;
    padding-right: 25rpx;
}

.packbox {
    margin-top: 26rpx;
}

.packclass {
    padding-left: 25rpx;
    padding-right: 25rpx;
    background: #fff;
    border-radius: 20rpx;
    display: flex;
    justify-content: space-between;
    height: 100rpx;
}

.packname {
    font-size: 28rpx;
    color: #666666;
    line-height: 100rpx;
}

.arrowImg {
    line-height: 100rpx;
}

.arrowImg image {
    width: 32rpx;
    height: 24rpx;
}

.dredgebox {
    margin-top: 24rpx;
}

.dre {
    width: 100%;
    height: 100rpx;
    background: white;
    border-radius: 20rpx;
    display: flex;
    justify-content: space-between;
}

.dre_left {
    color: #666666;
    font-size: 28rpx;
    line-height: 100rpx;
    margin-left: 24rpx;
}

.dre_right {
    margin-right: 24rpx;
    line-height: 96rpx;
}

.wx-switch-input {
    width: 90rpx!important;
    height: 50rpx!important;
    background: #5CC9AC!important;
    border: #ccc!important;
}

.wx-switch-input::before {
    width: 90rpx!important;
    height: 50rpx!important;
}

.wx-switch-input::after {
    width: 45rpx!important;
    height: 50rpx!important;
}

.footbox {
    width: 100%;
    position: fixed;
    left: 0rpx;
    bottom: 90rpx;
    padding-left: 25rpx;
    padding-right: 25rpx;
}

.add {
    width: 700rpx;
    background: #5CC9AC;
    height: 96rpx;
    border-radius: 20rpx;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 30rpx;
}

.addImg {
    width: 45rpx;
    height: 45rpx;
    border-radius: 50%;
    margin-right: 19rpx;
}

.addImg image {
    width: 100%;
    height: 100%;
}