page {
    width: 100%;
    height: 100%;
    background: #f8f8f8;
}

.oval {
    padding-left: 25rpx;
    padding-right: 25rpx;
}

.packbox {
    margin-top: 26rpx;
}

.packclass {
    padding-left: 25rpx;
    padding-right: 25rpx;
    background: #fff;
    border-radius: 20rpx;
    display: flex;
    justify-content: space-between;
    height: 100rpx;
}

.packname {
    font-size: 28rpx;
    color: #666666;
    line-height: 100rpx;
}

.packRight {
    padding-top: 20rpx;
}

.arrowImg image {
    width: 32rpx;
    height: 24rpx;
}