page {
    width: 100%;
    height: 100%;
    font-family: "Microsoft YaHei";
    background: #fafcfb;
}

.oval {
    padding: 20rpx;
}

.header {
    display: flex;
    font-size: 30rpx;
    width: 100%;
    font-weight: bold;
}

.type {
    width: 30rpx;
    padding-right: 20rpx;
}

.item {
    width: 145rpx;
    line-height: 60rpx;
}

.connotation {
    flex: 1;
    padding-left: 10rpx;
    line-height: 60rpx;
}

.list {
    display: flex;
    font-size: 30rpx;
    color: #333;
    width: 100%;
}

.list_right {
    flex: 1;
    width: 100%;
    display: flex;
}

.serial {
    width: 30rpx;
    line-height: 60rpx;
    padding-right: 20rpx;
}

.cont {
    flex: 1;
    padding-left: 10rpx;
}