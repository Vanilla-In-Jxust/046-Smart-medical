page {
    width: 100%;
    height: 100%;
    background: #f3f3f3;
}

.personal {
    padding: 0 20rpx;
    color: #585858;
    font-size: 32rpx;
    line-height: 72rpx;
}

.intr {
    width: 100%;
    background: white;
}

.oval {
    padding: 15rpx 20rpx;
    display: flex;
    border-bottom: 1rpx solid #f4f4f4;
    justify-content: space-between;
}

.oval_left {
    padding-right: 20rpx;
    color: #656565;
    font-size: 30rpx;
    padding-left: 10rpx;
    line-height: 60rpx;
}

.oval_right {
    flex: 1;
    text-align: right;
}

.oval_right input {
    font-size: 31rpx;
}

.picker {
    font-size: 30rpx;
}

.crowd {
    padding: 25rpx;
    display: flex;
    flex-wrap: wrap;
}

.classify {
    width: 210rpx;
    font-size: 30rpx;
    border-radius: 10rpx;
    text-align: center;
    padding-top: 15rpx;
    padding-bottom: 10rpx;
    margin-bottom: 15rpx;
    margin-right: 31rpx;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    word-break: break-all;
}

.selct_no {
    background: #f7f7f7;
    color: #3a3a3a;
    border: 1rpx solid #f7f7f7;
}

.select_yes {
    background: #e8f6f7;
    color: #489cdc;
    border: 1rpx solid #489cdc;
}

.classify:nth-of-type(3n+0) {
    margin-right: 0rpx;
}

.housebox {
    display: flex;
    justify-content: flex-end;
    padding-top: 10rpx;
}

.sel {
    display: flex;
    padding-right: 20rpx;
}

.house {
    display: flex;
}

.selectImg {
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    margin-right: 15rpx;
}

.selectImg image {
    width: 100%;
    height: 100%;
}

.show {
    display: block;
}

.hide {
    display: none;
}

.hei {
    height: 140rpx;
}

.foot {
    width: 100%;
    height: 140rpx;
    background: #f3f3f3;
    position: fixed;
    left: 0rpx;
    bottom: 0rpx;
    z-index: 1;
}

.signfont {
    width: 100%;
    height: 100rpx;
    font-size: 40rpx;
    color: white;
    line-height: 100rpx;
    text-align: center;
    position: fixed;
    bottom: 0;
}