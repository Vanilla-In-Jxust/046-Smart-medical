.doc-vienna {
    width: 702rpx;
    margin: 24rpx 24rpx 0 24rpx;
    padding-top: 24rpx;
    border-radius: 30rpx;
    background: #fff;
}

.doc-tit {
    position: relative;
    text-indent: 26rpx;
    font-size: 32rpx;
    color: #333;
}

.doc-tit::after {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    margin: auto 0;
    width: 8rpx;
    height: 30rpx;
    border-radius: 4rpx;
    background: #0087E0;
}

.doc-vienna-list {
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
    margin-top: 24rpx;
}

.doc-vienna-item {
    display: flex;
    align-items: center;
    width: 292rpx;
    height: 70rpx;
    margin-bottom: 24rpx;
    padding: 30rpx 16rpx;
    border-radius: 30rpx;
    background: #f7f7f7;
}

.doc-vienna-item image {
    width: 70rpx;
    height: 70rpx;
    margin-right: 12rpx;
}

.doc-service {
    display: flex;
    justify-content: space-around;
    padding: 24rpx 0;
    font-size: 24rpx;
    color: #333;
}

.doc-ser-item {
    position: relative;
    text-align: center;
}

.doc-ser-item image {
    width: 80rpx;
    height: 80rpx;
    margin-bottom: 20rpx;
}

.doc-data {
    display: flex;
    flex-wrap: wrap;
    padding-top: 24rpx;
}

.doc-data-item {
    width: 33.33%;
    margin-bottom: 28rpx;
    font-size: 40rpx;
    text-align: center;
}

.doc-data-tip {
    margin-top: 6rpx;
    font-size: 24rpx;
}

.organ-top {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    width: 710rpx;
    padding: 68rpx 25rpx;
    background: #0087E0;
    font-size: 32rpx;
    color: #fff;
}

.logo {
    width: 122rpx;
    height: 122rpx;
    margin-right: 24rpx;
    border-radius: 50%;
}

.organ-t {
    display: flex;
    align-items: center;
}

.label {
    font-size: 24rpx;
    color: #0087E0;
    background: #fff;
    padding: 2rpx 14rpx;
    border-radius: 5rpx;
    margin-left: 25rpx;
}

.organ-tip {
    margin-top: 20rpx;
    font-size: 26rpx;
}

.organ-mask {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: rgba(0,0,0,.5);
    z-index: 997;
}

.organ-pupop {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    width: 516rpx;
    height: 576rpx;
    z-index: 998;
}

.organ-bg {
    width: 100%;
}

.organ-text {
    position: absolute;
    top: 265rpx;
    left: 22rpx;
    width: 470rpx;
    text-align: center;
}

.organ-title {
    font-size: 26rpx;
    color: #333;
}

.link {
    width: 100%;
    overflow: hidden;
    margin-top: 20rpx;
    font-size: 26rpx;
    color: #0087E0;
}

.organ-btn {
    width: 100%;
    height: 80rpx;
    margin-top: 70rpx;
    line-height: 80rpx;
    font-size: 26rpx;
    color: #fff;
}

.close-p {
    width: 40rpx;
    height: 40rpx;
    margin-top: 30rpx;
}