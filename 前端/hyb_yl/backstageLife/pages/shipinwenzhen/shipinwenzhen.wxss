page {
    width: 100%;
    height: 100%;
    background: #FFFFFF;
}

.oval {
    padding: 25rpx;
}

.headbox {
    height: 380rpx;
    border-radius: 20rpx;
    background-size: 530rpx 430rpx;
    background-position: right;
    background-repeat: no-repeat;
}

.bigTitle {
    font-size: 45rpx;
    font-weight: bold;
    padding-left: 35rpx;
    padding-top: 35rpx;
    color: #FFFFFF;
    margin-bottom: 30rpx;
}

.smallTitlte {
    color: #daddff;
    font-size: 22rpx;
    margin-left: 32rpx;
}

.smallfont {
    margin-bottom: 15rpx;
}

.smallfont text {
    margin-right: 15rpx;
}

.commonbox {
    padding-top: 50rpx;
}

.commTitlebox {
    display: flex;
    justify-content: space-between;
    margin-bottom: 30rpx;
}

.title_left {
    border-left: 5rpx solid #0088de;
    padding-left: 15rpx;
    font-weight: bold;
    color: #333;
    font-size: 32rpx;
}

.lookeval {
    color: #0088de;
    font-size: 28rpx;
}

.evalCont {
    font-size: 28rpx;
    color: #333;
    padding-bottom: 40rpx;
    border-bottom: 1rpx solid #eaeaea;
}

.czlist {
    line-height: 40rpx;
}

.now {
    font-size: 30rpx;
    color: #FFFFFF;
    width: 160rpx;
    height: 60rpx;
    border: 1rpx solid #FFFFFF;
    text-align: center;
    line-height: 60rpx;
    border-radius: 10rpx;
    margin-top: 80rpx;
    margin-left: 32rpx;
}

rich-text {
    display: block;
    font-size: 28rpx;
    color: #353535;
    letter-spacing: 1rpx;
}

.p_class {
    line-height: 46rpx;
    text-indent: 60rpx;
    margin-bottom: 20rpx;
}

.pho {
    display: block;
    width: 670rpx!important;
    height: 330rpx;
    border-radius: 20rpx;
}

.zhe {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.4);
    display: flex;
    justify-content: center;
    align-items: center;
}

.zhe .cont {
    width: 80%;
    background: #fff;
    border-radius: 10rpx;
    padding: 60rpx 40rpx;
}

.rule_box {
    height: 418rpx;
    overflow: hidden;
    overflow-y: auto;
}

.zhe .title {
    font-size: 36rpx;
    text-align: center;
    font-weight: bold;
    color: #666;
    margin-bottom: 30rpx;
}

.rule_btn {
    width: 100%;
    height: 88rpx;
    font-size: 32rpx;
    color: #fff;
    text-align: center;
    line-height: 88rpx;
    border-radius: 20rpx;
    margin: 30rpx auto;
    background: #004cbe;
}

.W_name {
    width: 100%;
    height: 100rpx;
    border-bottom: 1rpx solid #f0f0f0;
    background: #fff;
    padding: 0;
    display: flex;
    justify-content: space-between;
}

.W_imgName1 {
    height: 100rpx;
    float: left;
    line-height: 100rpx;
    text-indent: 30rpx;
    font-size: 30rpx;
    color: #333;
    white-space: nowrap;
}