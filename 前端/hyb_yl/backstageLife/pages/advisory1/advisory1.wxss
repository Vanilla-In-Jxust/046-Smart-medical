@import "../../../../wxss/15.wxss";

page {
    background: #fff;
}

.auto {
    padding: 0 30rpx;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
    color: #333;
    font-size: 34rpx;
    line-height: 50rpx;
}

.title1 {
    font-weight: bold;
    border-bottom: 10rpx;
    margin-top: 20rpx;
}

.wxParse-p {
    margin-top: 20rpx;
}

.IDclick {
    width: 300rpx;
    height: 60rpx;
    float: left;
    background: #fff;
    display: flex;
    align-items: center;
    margin-top: 120rpx;
    margin-bottom: 120rpx;
}

.IDclick>image {
    width: 45rpx;
    height: 40rpx;
    z-index: 999;
    margin-left: 20rpx;
}

.IDclick::after {
    border: none;
}

.share_view {
    display: inline-block;
}

.for {
    margin-top: 20rpx;
}

.text_view {
    font-size: 32rpx;
    color: #333;
    line-height: 52rpx;
}

.img_view {
    width: 690rpx;
    border-radius: 8rpx;
    margin: 20rpx 0;
}

img {
    width: 690rpx;
    height: 100%;
    margin: 0 auto;
}