.tabBar-icon {
    width: 54rpx;
    height: 54rpx;
}

.tabBar {
    width: 100%;
    height: 90rpx;
    position: fixed;
    bottom: 0;
    padding: 10rpx;
    margin-left: -4rpx;
    background: #FFFFFF;
    font-size: 20rpx;
    color: #8A8A8A;
    box-shadow: 6rpx 6rpx 6rpx 6rpx #aaa;
}

.tabBar-item {
    overflow: hidden;
    float: left;
    width: 24%;
    text-align: center;
}

.tabBartext {
    color: #6ec7ff;
}

.navigator-hover {
    background-color: #FFFFFF;
    opacity: 1;
}