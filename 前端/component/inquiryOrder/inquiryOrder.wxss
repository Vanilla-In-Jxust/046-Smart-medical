.nosuccess {
    background: white;
    margin-top: 20rpx;
    border-radius: 10rpx;
}

.expertbox {
    font-size: 30rpx;
    display: flex;
    padding: 20rpx;
    border-bottom: 1rpx solid #f1f1f1;
}

.inquiry {
    color: #333;
    font-weight: bold;
}

.price {
    margin-left: 30rpx;
    color: #fec67c;
    font-weight: bold;
}

.consult {
    flex: 1;
    text-align: right;
    font-size: 30rpx;
    color: #959595;
}

.patientList {
    padding: 5rpx 20rpx;
    border-bottom: 1rpx solid #eaeaea;
    font-size: 28rpx;
}

.list {
    display: flex;
    width: 100%;
    line-height: 60rpx;
}

.list_left {
    color: #333;
}

.list_right {
    color: #999;
    padding-left: 20rpx;
    flex: 1;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.time {
    padding: 20rpx;
    font-size: 24rpx;
    color: #cbcbcb;
}